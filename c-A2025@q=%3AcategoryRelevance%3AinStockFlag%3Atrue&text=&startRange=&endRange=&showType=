	let oldValue = 0
	let newValue = 0
	let isScrolling;
	let userScrolled10Percent = false;
	let userScrolled25Percent = false;
	let userScrolled50Percent = false;
	let userScrolled75Percent  = false;
	let userScrolled90Percent  = false;
	let userScrolled100Percent  = false;
	window.addEventListener('scroll', function ( event ) {

		window.clearTimeout( isScrolling );
		isScrolling = setTimeout(function() {
		var scrollTop = $(window).scrollTop();
		var docHeight = $(document).height();
		var winHeight = $(window).height();
		var scrollPercent = (scrollTop) / (docHeight - winHeight);
		var scrollPercentRounded = Math.round(scrollPercent*100);
		newValue = window.pageYOffset;
		if (oldValue < newValue) {
			getScrollRange(scrollPercentRounded);
		}
		oldValue = newValue;
		}, 66);

	}, false);

function removeDuplicateScrollDepthEvent(scrollThresholdValue) {
	dataLayer.forEach((element,index) => {
		if(element.event && element.event == 'gtm.scrollDepth'){
			dataLayer.splice(index,1)
		}
	});
	dataLayer.push({
		event: "gtm.scrollDepth",
		scrollThreshold: scrollThresholdValue,
		pageName: "CATEGORY",
		categoryName: "Bags & Wallets",
		category: categoryCode,
		scrollUnits: "percent",
		scrollDirection: "vertical"
	})
}
function getScrollRange(scrollposition) {
	var currentScrollPosition = scrollposition;
	var scrollThresholdValue = "";
	if(currentScrollPosition >= 10 && currentScrollPosition < 25 && !userScrolled10Percent) {
		scrollThresholdValue = 10;
		removeDuplicateScrollDepthEvent(scrollThresholdValue);
		userScrolled10Percent = true;
	}
	if(currentScrollPosition >= 25 && currentScrollPosition < 50 && !userScrolled25Percent) {
		scrollThresholdValue = 25;
		removeDuplicateScrollDepthEvent(scrollThresholdValue);
		userScrolled10Percent = true;
		userScrolled25Percent = true;
	}
	if(currentScrollPosition >= 50 && currentScrollPosition < 75 && !userScrolled50Percent) {
		scrollThresholdValue = 50;
		removeDuplicateScrollDepthEvent(scrollThresholdValue);
		userScrolled10Percent = true;
		userScrolled25Percent = true;
		userScrolled50Percent = true;
	}
	if(currentScrollPosition >= 75 && currentScrollPosition < 90 && !userScrolled75Percent) {
		scrollThresholdValue = 75;
		removeDuplicateScrollDepthEvent(scrollThresholdValue);
		userScrolled10Percent = true;
		userScrolled25Percent = true;
		userScrolled50Percent = true;
		userScrolled75Percent = true;
	}
	if(currentScrollPosition >= 90 && currentScrollPosition < 100 && !userScrolled90Percent) {
		scrollThresholdValue = 90;
		removeDuplicateScrollDepthEvent(scrollThresholdValue);
		userScrolled10Percent = true;
		userScrolled25Percent = true;
		userScrolled50Percent = true;
		userScrolled75Percent = true;
		userScrolled90Percent = true;
		
	}
	if(currentScrollPosition >= 100 && !userScrolled100Percent) {
		scrollThresholdValue = 100;
		removeDuplicateScrollDepthEvent(scrollThresholdValue);
		userScrolled10Percent = true;
		userScrolled25Percent = true;
		userScrolled50Percent = true;
		userScrolled75Percent = true;
		userScrolled90Percent = true;
		userScrolled100Percent = true;
	}
}

var firstTime=0
	function onPromoClick(promoObj) {
		// alert("gfhhg")
		var currentPage="NA"
	var prevpage="NA"
	if(sessionStorage.getItem("prevPage")){
	let prevPageArray=JSON.parse(sessionStorage.getItem("prevPage"))
	if(prevPageArray.length >=2){
		prevpage=prevPageArray[prevPageArray.length-2]
	}else if(prevPageArray.length ==1){
		prevpage="NA"
	}
}
if(sessionStorage.getItem("prevPage")){
	let prevPageArray=JSON.parse(sessionStorage.getItem("prevPage"))
	if(prevPageArray.length >=1){
		currentPage=prevPageArray[prevPageArray.length-1]
	}else if(prevPageArray.length ==1){
		currentPage="NA"
	}
}

  var currentDevice = ""
var phoneOrNot = !!navigator.maxTouchPoints;
let phoneNum = "";

if (phoneOrNot) {
	if (navigator.userAgent.match(/Android/i)) {
		currentDevice = "android"
	} else {
		currentDevice = "ios"
	}
} else {
	currentDevice = "desktop"
}

                                          
var visitorType = "";
if (sessionStorage.getItem("isLoggedIn") == "true") {
	visitorType = "loggedin"
} else {
	visitorType = "guest"
}

                                    dataLayer.push({
                                        ecommerce: null
                                    });
								let userID="NA"
								 if (sessionStorage.getItem("isLoggedIn") == "true") {
									userID=localStorage.getItem("UserIdkey")
									
									} else {
										// visitorType = "guest"
									}
			
if(!firstTime)
{				
			dataLayer.push({
	'event': 'select_promotion',
	'userID': userID, 
	'screen_name':currentPage, 
	'previous_screen_name': prevpage, 
	'visitor_type': visitorType, 
	'platform': currentDevice, 
	'pagepath': window.location.pathname, 
	'ecommerce': {
		'items': [
		{
		'promotion_id': promoObj.id, 
		'promotion_name':promoObj.name, 
		'creative_name': promoObj.creative, 
		'creative_slot': parseInt(promoObj.pos)+1, 
		}]
	}
	});
	firstTime=1
}
		  dataLayer.push({
		    'event': 'promotionClick',
		    'ecommerce': {
		      'promoClick': {
		        'promotions': [
		         {
		           'id': promoObj.id,                         // Name or ID is required.
		           'name': promoObj.name,
		           'creative': promoObj.creative,
		           'position': promoObj.pos,
				   'pageType': `CATEGORY`
		         }]
		      }
		    },
		    'eventCallback': function() {
		      document.location = promoObj.destinationUrl;
		    }
		  });
		}
	
	
(function() {
	var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
	ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
	var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
})();


function trackAddToCart_google(productCode, quantityAdded) {
	dataLayer.push(['_trackEvent', 'Cart', 'AddToCart', productCode, quantityAdded]);
}

function trackUpdateCart(productCode, initialQuantity, newQuantity) {
	if (initialQuantity != newQuantity) {
		if (initialQuantity > newQuantity) {
			dataLayer.push(['_trackEvent', 'Cart', 'RemoveFromCart', productCode, initialQuantity - newQuantity]);
		} else {
			dataLayer.push(['_trackEvent', 'Cart', 'AddToCart', productCode, newQuantity - initialQuantity]);
		}
	}
}

function trackRemoveFromCart(productCode, initialQuantity) {
	dataLayer.push(['_trackEvent', 'Cart', 'RemoveFromCart', productCode, initialQuantity]);
}

window.mediator.subscribe('trackAddToCart', function(data) {
	
	if (data.productCode && data.quantity)
	{
		trackAddToCart_google(data.productCode, data.quantity);
	}
});

window.mediator.subscribe('trackUpdateCart', function(data) {
	if (data.productCode && data.initialCartQuantity && data.newCartQuantity)
	{
		trackUpdateCart(data.productCode, data.initialCartQuantity, data.newCartQuantity);
	}
});

window.mediator.subscribe('trackRemoveFromCart', function(data) {
	if (data.productCode && data.initialCartQuantity)
	{
		trackRemoveFromCart(data.productCode, data.initialCartQuantity);
	}
}); 



$(document).on('click', '.carousal-button', function(e) {
	
	 var link= $(this).attr("href");
	 var id=$(this).attr("data-select-uid");
	 var name=$(this).attr("data-select-name");
	 var position=$(this).parents('li').find('input').val();
	 var creative=$(this).attr("data-select-creative");
	 var promoObj = {id:id, name:name, pos:position, creative:creative, destinationUrl:link};

	 onPromoClick(promoObj);
});

$(document).on('click', '.fav-banner-button', function(e) {
	
	 var link= $(".fav-banner-button").attr("href");
	 var id=$(".fav-banner-button").attr("data-select-uid");
	 var name=$(".fav-banner-button").attr("data-select-name");
	 var position=$(".fav-banner-button").attr("data-select-position");
	 var creative=$(".fav-banner-button").attr("data-select-creative");
	 var promoObj = {id:id, name:name, pos:position, creative:creative, destinationUrl:link};

	 onPromoClick(promoObj);
});

$(document).on('click', '.latestFromCmsComponentATag', function(e) {
	
	 var link= $(this).attr("href");
	 var id=$(this).attr("data-select-uid");
	 var name=$(this).attr("data-select-name");
	 var position=$(this).attr("data-select-position");
	 var creative=$(this).attr("data-select-creative");
	 var promoObj = {id:id, name:name, pos:position, creative:creative, destinationUrl:link};

	 onPromoClick(promoObj);
});

$(document).on('click', '.offersZoneCmsComponentATag', function(e) {
	
	 var link= $(this).attr("href");
	 var id=$(this).attr("data-select-uid");
	 var name=$(this).attr("data-select-name");
	 var position=$(this).attr("data-select-position");
	 var creative=$(this).attr("data-select-creative");
	 var promoObj = {id:id, name:name, pos:position, creative:creative, destinationUrl:link};

	 onPromoClick(promoObj);
});

$(document).on('click', '.brandsWeLoveHomePageCmsComponentATag', function(e) {
	
	 var link= $(this).attr("href");
	 var id=$(this).attr("data-select-uid");
	 var name=$(this).attr("data-select-name");
	 var position=$(this).attr("data-select-position");
	 var creative=$(this).attr("data-select-creative");
	 var promoObj = {id:id, name:name, pos:position, creative:creative, destinationUrl:link};
// alert("sdffds")
	 onPromoClick(promoObj);
});
var firstTime=0
function onPromoClick(promoObj) {
	var currentPage="NA"
	var prevpage="NA"
	if(sessionStorage.getItem("prevPage")){
	let prevPageArray=JSON.parse(sessionStorage.getItem("prevPage"))
	if(prevPageArray.length >=2){
		prevpage=prevPageArray[prevPageArray.length-2]
	}else if(prevPageArray.length ==1){
		prevpage="NA"
	}
}
if(sessionStorage.getItem("prevPage")){
	let prevPageArray=JSON.parse(sessionStorage.getItem("prevPage"))
	if(prevPageArray.length >=1){
		currentPage=prevPageArray[prevPageArray.length-1]
	}else if(prevPageArray.length ==1){
		currentPage="NA"
	}
}

  var currentDevice = ""
var phoneOrNot = !!navigator.maxTouchPoints;
let phoneNum = "";

if (phoneOrNot) {
	if (navigator.userAgent.match(/Android/i)) {
		currentDevice = "android"
	} else {
		currentDevice = "ios"
	}
} else {
	currentDevice = "desktop"
}

                                          
var visitorType = "";
if (sessionStorage.getItem("isLoggedIn") == "true") {
	visitorType = "loggedin"
} else {
	visitorType = "guest"
}

                                    dataLayer.push({
                                        ecommerce: null
                                    });
								let userID="NA"
								 if (sessionStorage.getItem("isLoggedIn") == "true") {
									userID=localStorage.getItem("UserIdkey")
									
									} 
				
if(!firstTime)
{	
					dataLayer.push({
	'event': 'select_promotion',
	'userID': userID, 
	'screen_name':currentPage, 
	'previous_screen_name': prevpage, 
	'visitor_type': visitorType, 
	'platform': currentDevice, 
	'pagepath': window.location.pathname, 
	'ecommerce': {
		'items': [
		{
		'promotion_id': promoObj.id, 
		'promotion_name':promoObj.name, 
		'creative_name': promoObj.creative, 
		'creative_slot':parseInt(promoObj.pos)+1, 
		}]
	}
	});
	firstTime=1
}					
	

	  dataLayer.push({
	    'event': 'promotionClick',
	    'ecommerce': {
	      'promoClick': {
	        'promotions': [
	         {
	           'id': promoObj.id,                         // Name or ID is required.
	           'name': promoObj.name,
	           'creative': promoObj.creative,
	           'position': promoObj.pos,
			   'pageType': `CATEGORY`
	         }]
	      }
	    },
	    'eventCallback': function() {
	      document.location = promoObj.destinationUrl;
	    }
	  });
	}

