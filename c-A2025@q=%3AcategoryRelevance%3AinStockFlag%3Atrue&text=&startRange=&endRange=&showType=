	function createFilterValues(str) {
		// alert("sdfsdfghf")
		if(str) {
			var getString  = str.split("|");
			var arrayItem = [];
			var filters = {};
			var ctnames=""
			var headerName=""
			for(var i=0;i<getString.length;i++) {
				var rowItem = getString[i];
				var spitRowVal = rowItem.split(":");
				for(var j=0;j<spitRowVal.length;j++) {
					filters[spitRowVal[0]] = {
						filterValues: spitRowVal[1]
					}
				}
				headerName=headerName+spitRowVal[0]+","

				ctnames+=spitRowVal[1]+","
			}
			
			//msite all flters at once
				var currentDevice = ""
		var phoneOrNot = !!navigator.maxTouchPoints;
		let phoneNum = "";
		
	if (phoneOrNot) {
		if (navigator.userAgent.match(/Android/i)) {
			currentDevice = "android"
		} else {
			currentDevice = "ios"
		}
	} else {
		currentDevice = "desktop"
	}
	dataLayer.push({
		ecommerce: null
	});
	var customer_type="NA"
	var user_type= "NA"
	var visitorType = "";

	if (sessionStorage.getItem("isLoggedIn") == "true") {
		visitorType = "loggedin"
		 user_type=localStorage.getItem("user_type_value") || "NA"
		 if(localStorage.getItem("user_type_value")){
	
	}
	} else {
		visitorType = "guest"
	}
	
		let prevpage="NA"
		var screenName="NA";
	
// alert(sessionStorage.getItem("prevPage"))
if(sessionStorage.getItem("prevPage")){
	let prevPageArray=JSON.parse(sessionStorage.getItem("prevPage"))
	if(prevPageArray.length>=1){
		screenName=prevPageArray[prevPageArray.length-1]
	}
	if(prevPageArray.length >=2){
		prevpage=prevPageArray[prevPageArray.length-2]
	}else if(prevPageArray.length ==1){
		prevpage="NA"
	}
}	
let userID="NA"
if (sessionStorage.getItem("isLoggedIn") == "true") {
	userID=localStorage.getItem("UserIdkey")
	
	} 
			$.ajax({
                      url : "/my-account/isOrderPresent",
                      type : "GET",
                      async: false,
                      success : function(res) {
				if(res)
				{
					if (sessionStorage.getItem("isLoggedIn") == "true") {
						customer_type="existing_customer"
						}
						else{
							customer_type="NA"
						}
					
				}
				else{
						if (sessionStorage.getItem("isLoggedIn") == "true") {
						customer_type="new_customer"
						}
						else{
							customer_type="NA"
						}

				}
				var member_status="Non member"
				if(localStorage.getItem("GA_member_status")){
					if (sessionStorage.getItem("isLoggedIn") == "true") {
						member_status=localStorage.getItem("GA_member_status")
					}
				}
				$.ajax({
                      url : "/dy/customer/getUserDetails",
                      type : "GET",
                      async: false,
                      success : function(data) {
					dataLayer.push({'event':'apply_filter',
						'cta_name':ctnames.slice(0, -1).toLowerCase(),
						'header_name':headerName.slice(0, -1).toLowerCase(),
						'userID':userID,
						'member_status':member_status , 
						'screen_name':screenName,
						'previous_screen_name':prevpage
						});
						
						// alert("Ads")
					  }
				})

	  }})
			
			dataLayer.push({
				'event': 'filter_click',
				'filterNames': filters,
				'categoryName': "Bags & Wallets",
				'category': categoryCode
			});
		}
		
	}	


