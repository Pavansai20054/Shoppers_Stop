	$(document).on('click','.filter-aside-mobile .filter-inner-mobile button.btn-apply', function(e){ 
		var strValues = "";
		$('.filter-aside-mobile .filter-inner-mobile .filter-tab-content-mob .filter-wpr .ex-checkbox .facet-checkbox-mobile:checked').each(function(i, obj) {
			if(strValues.indexOf($(this).closest('.filter-wpr').attr('filter-name'))>-1) {
				strValues += ",";
				strValues += $(this).attr('data-facet-value');
			} else {
				if(strValues.length>0) {
					strValues += "|";
				}
				strValues += $(this).closest('.filter-wpr').attr('filter-name');
				strValues += ":";
				strValues += $(this).attr('data-facet-value');
			}
		});
		createFilterValues(strValues)
	});

