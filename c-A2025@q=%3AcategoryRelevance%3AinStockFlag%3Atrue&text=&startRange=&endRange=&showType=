$(document).ready(function() {
        var url = window.location.href.split("/");
        let categoryids_one="";
        let idsss_new="";
        if(url[4])
        {
        categoryids_one=url[4].split("?");
        idsss_new=categoryids_one[0].split("-");
        console.log(idsss_new);
        }

        // console.log(=="brands")
        console.log("asdasdsdvfsv")
        console.log(url);
        // console.log(idsss_new[1].length)

        var BargainsorLuxe1= localStorage.getItem("BargainsorLuxe");
        var BargainsorLuxe_id= localStorage.getItem("BargainsorLuxe_id");
        console.log(url[3].split("?")[0]=="brands")
        console.log(url[3].split("?")[0])
        if(url[3].split("?")[0]=="brands")
        {
                console.log("inside brands")
                dataLayer.push({
                "event":"Category Viewed",
                "ecommerce":{
                        "currencyCode":"INR",
                        "category":{
                
