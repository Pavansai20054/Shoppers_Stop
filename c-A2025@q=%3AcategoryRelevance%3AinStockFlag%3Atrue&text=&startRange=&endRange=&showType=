	_satellite.track('PS_book_now_click');
});

//on click of personal shoppers continue button in store finder page
$(document).on('click', '.store_leftside #storeFinderForm #storecontinueBtnId', function(e) {
	var macBookingId = $('#macBeautyId').val() != null  ? $('#macBeautyId').val() : '798330002'; 
	if($('#bookingTypeId').val() == macBookingId){
		_satellite.track('mac_store_select_continue');
	}
	else{
		_satellite.track('ps_store_select_continue');
	}

});

