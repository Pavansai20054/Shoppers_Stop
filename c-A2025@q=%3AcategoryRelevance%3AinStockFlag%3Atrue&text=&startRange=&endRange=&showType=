//Call this func when user clicks on "product" from PLP 
//Product positioning on Grid Page
  $(document).on('click','.product-grid .pro-box .li-inner',function(e){
	  if (e.target.nodeName!='BUTTON' && e.target.id!='removewishlistId'){
	  var productPosition=$(this).siblings('.nApPostion').val();
	  var productRowPosition=parseInt(((productPosition-1)/4)+1);
	  var productColumnPosition=productPosition-4*(productRowPosition-1);
      localStorage.setItem("productIndex", "R"+productRowPosition+"C"+productColumnPosition);
      localStorage.setItem("viewType", "Grid");
	  }
  });

//Product positioning on List Page
  $(document).on('click','.pro-box-list .styleImageData .bx-viewport li',function(e){
	  var productPosition=$(this).parents('li').find('.nApPostion').val();
      localStorage.setItem("productIndex", "R"+productPosition);
      localStorage.setItem("viewType", "List");	 
  });
     

//Call this function when user clicks on promotional banners
$(function(){
	      $('.carousal-button,.fav-banner-button,.latestFromCmsComponentATag,.offersZoneCmsComponentATag,.brandsWeLoveHomePageCmsComponentATag,.simple-responsive-banner-component,.style-hub-img,.cat-best-brands-inn ul li,.new-arrivals ul li .pic,.square-thumb ul li .lazyloaded,.customRow ul li a .carousal-button,.offer-zone ul li a,.discover_blog ul li div .media,.first_update ul li div .media,.bank-offer-main a,.giftmedia .media a').click(function(){
            localStorage.setItem("productFindingMethod", "internal promotion");
        	localStorage.setItem("isBannerClicked", true);
            digitalData["dataMedia"] = $(this).find('img').attr('data-media');
	       _satellite.track('int-camp');
       }); 
});
 
