	/**
	 * Call this function when a user clicks on a product link. This function uses the event
	 * callback datalayer variable to handle navigation after the ecommerce data has been sent
	 * to Google Analytics.
	 * @param {Object} productObj An object representing a product.
	 * Currently this one is being called for Search PLP
	 */
	
	 var clickedProduct,productObj,productPosition;
	 $(document).on('click', '.product-wpr ul>li a.thumb,.product-wpr ul>li .pro-info a', function(e) {
		e.preventDefault();
	    productPosition = $(this).closest('li').find('input').val();
		 var productCode = $(this).attr('href').split('/p-')[1].split('?')[0];
		 productCode = productCode.split('/').pop();
	    var clickedProductDetailsUrl ="/cart/productBasicDetails";
	    var url = $(this).attr('href');
	   		
		   $.ajax({
			    data : {
			     'productCode' : productCode
			    },
			    type : 'Get',
			    url : clickedProductDetailsUrl,
			    success : HomeObject,
			    error : function(jqXHR, textStatus, errorThrown) {
			     console.log(JSON.stringify(jqXHR));
			     console.log("AJAX error: " + textStatus + ' : ' + errorThrown);
			    },
                complete: function () {
                    window.location.href = url;
                }
		   });
		   
		   
				   function HomeObject(productObj) {
					console.log("abcdef")
					// alert("bsfvsdsffsd")
								dataLayer.push({ ecommerce: null }); 
				var currentDevice = ""
		var phoneOrNot = !!navigator.maxTouchPoints;
	let phoneNum = "";

	if (phoneOrNot) {
		if (navigator.userAgent.match(/Android/i)) {
			currentDevice = "android"
		} else {
			currentDevice = "ios"
		}
	} else {
		currentDevice = "desktop"
	}

	dataLayer.push({
		ecommerce: null
	});
	var visitorType = "";
	if (sessionStorage.getItem("isLoggedIn") == "true") {
		visitorType = "loggedin"
	} else {
		visitorType = "guest"
	}
	if (sessionStorage.getItem("isLoggedIn") == "true") {
		phoneNum = "+91" + localStorage.getItem("userNumber")
	} else {
		phoneNum = "NA"

	}
		let prevpage="NA"
// alert(sessionStorage.getItem("prevPage"))
if(sessionStorage.getItem("prevPage")){
	let prevPageArray=JSON.parse(sessionStorage.getItem("prevPage"))
	if(prevPageArray.length >=2){
		prevpage=prevPageArray[prevPageArray.length-2]
	}else if(prevPageArray.length ==1){
		prevpage="NA"
	}
}	
let userID="NA"
if (sessionStorage.getItem("isLoggedIn") == "true") {
	userID=localStorage.getItem("UserIdkey")
	
	} else {
		// visitorType = "guest"
	}
	
	$.ajax({
			type : "GET",
			url : window.location.origin+"/dy/customer/getL4Code?productCode="+productObj.code,
			success : function(response) {
      
      dataLayer.push({
  'event': 'select_item',
  'userID': userID,
 'screen_name': 'product listing page',
 'previous_screen_name': prevpage, 
 'visitor_type':visitorType,
 'platform': currentDevice, 
 'pagepath': window.location.pathname,
  'ecommerce': {
    'items': [
     {    
       'item_name': productObj.name, 
       'item_id': productObj.code,
       'item_brand':productObj.brandCode,
       'currency': 'INR', 
      'discount': "NA",
       'item_variant': productObj.baseOptions[0].selected.variantOptionQualifiers[0].value,
       		'item_category': response[0].l1Name , 
       'item_category2': response[0].l2Name, 
       'item_category3': response[0].l3Name ? response[0].l3Name :"NA",
       'item_category4': "NA", 
       'price': productObj.baseOptions[0].selected.priceData.value, 
       'item_list_name': 'NA', 
       'item_list_id': 'NA',
       'index':productPosition 
     }]
  }
});

			}})
				     dataLayer.push({
				    	 				    	
				       'event': 'productClick',
				       'ecommerce': {
				         'click': {
				           'actionField': {'list': 'Category Page'},      // Optional list property.
				           'products': [{
				             'name': productObj.name,                      // Name or ID is required.
				             'id': productObj.code,
				             'price': productObj.baseOptions[0].selected.priceData.value,
				             'brand': productObj.brandCode,
				             'category': productObj.primaryCategoryCode,	     //Product Category/Product Subcategory1/Product Subcategory2
					   	 	 'variant': productObj.baseOptions[0].selected.variantOptionQualifiers[0].value,
				             'position': productPosition
				            }]
				          }
				        },
				        'eventCallback': function() {
				        document.location = productObj.url
				        }
				     });   
				   }
	   }); 

	 $(document).on('click', '.category-page', function(e) {
			
		 var link= $(this).attr("href");
		 var id=$(this).attr("data-select-uid");
		 var name=$(this).attr("data-select-name");
		 var position=$(this).attr("data-select-position");
		 var creative=$(this).attr("data-select-creative");
		 var promoObj = {id:id, name:name, pos:position, creative:creative, destinationUrl:link};

		 onPromoClick(promoObj);
	});

	$(document).on('click', '.category-page-banner', function(e) {
		
		 var link= $(".category-page-banner").attr("href");
		 var id=$(".category-page-banner").attr("data-select-uid");
		 var name=$(".category-page-banner").attr("data-select-name");
		 var position=$(".category-page-banner").attr("data-select-position");
		 var creative=$(".category-page-banner").attr("data-select-creative");
		 var promoObj = {id:id, name:name, pos:position, creative:creative, destinationUrl:link};

		 onPromoClick(promoObj);
	});

	$(document).on('click', '.product-specific-banner', function(e) {
		
		 var link= $(".product-specific-banner").attr("href");
		 var id=$(".product-specific-banner").attr("data-select-uid");
		 var name=$(".product-specific-banner").attr("data-select-name");
		 var position=$(".product-specific-banner").attr("data-select-position");
