    localStorage.setItem("isBannerClicked", false);
    }

//calls this function when user click widgets in my account/wishlist tab.
$(".my-menu").find('a').click(function(){
	var myAccountPageName=$(this).text();
	trackPageRelatedData(myAccountPageName,myAccount);
 });
//calls this function when user clicks on my account/wishlist tab in the header.  
function myAccountWidgets(){
	var myAccountPageName = $('.my-menu').find('.active a').text();
	var myAccountChildWidgets = $('.my-menu').find('.active-child a').text();
	if(myAccountPageName){
		if( myAccountPageName == "Wishlist"){
			trackPageRelatedData(myAccountPageName,myAccountPageName);
		}else{
			trackPageRelatedData(myAccountPageName,myAccount);
		}
	}
	if(myAccountChildWidgets){
		if(myAccountChildWidgets == 'Profile'){
			myAccountChildWidgets = "My Profile";
		}else if(myAccountChildWidgets == 'First Citizen'){
			myAccountChildWidgets = "First Citizen Profile";
		}else if(myAccountChildWidgets == 'Reviews & Ratings'){
			myAccountChildWidgets = "Reviews and Ratings";
		}else if(myAccountChildWidgets == 'My Orders'){
			myAccountChildWidgets = "Orders";
		}
		
		
		trackPageRelatedData(myAccountChildWidgets,myAccount);
	}
	if(false)
	{
	trackPageRelatedData(myAccount,myAccount);
	}
}
//function for finding breadcrumbs data on every page.
function setBreadCrumbsData(count, breadcrumbs) {
    var breadcrumbsData = "";
    if (count > breadcrumbs.length) {
        count = breadcrumbs.length;
    }
    for (var i = 0; i < count; i++) {
        try {
            breadcrumbsData += breadcrumbs[i].replace(/"/g, "");
        } catch (e) {
            console.log('Unable to add breadcrumb', e);
        }
    }
    
    // return breadcrumbsData.endsWith("|") ? breadcrumbsData.slice(0, -1) : breadcrumbsData; /
    //SSLM-8005
    return (breadcrumbsData.length == breadcrumbsData.lastIndexOf("|") + 1) ? breadcrumbsData.slice(0, -1) : breadcrumbsData;
}

//function for finding breadcrumbs data on PDP page.
function setSectionBreadCrumbsData(count, breadcrumbs) {
	var breadcrumbsData = "";
    if (count > breadcrumbs.length) {
        count = breadcrumbs.length;
    }
    for (var i = 0; i < count; i++) {
        try {
            breadcrumbsData = breadcrumbs[i].replace("|", "");
        } catch (e) {
            console.log('Unable to add breadcrumb', e);
        }
    }
    //return breadcrumbsData.endsWith("|") ? breadcrumbsData.slice(0, -1) : breadcrumbsData;
    //SSLM-8005
    return (breadcrumbsData.length == breadcrumbsData.lastIndexOf("|") + 1) ? breadcrumbsData.slice(0, -1) : breadcrumbsData;
}

//function for checking breadcrumbs availability
function checkBreadcrumbsAvailability() {
	    
	        page["breadcrumbavailable"] = true; 
	    
	}
function trackPageData(pageName, subsection) {
  var entries = checkoutSignInPage.split("|");
  page["pageName"] = pageName;
  page["channel"] = entries[0] + "|" + entries[1];
 /*  page["subsection1"] = entries[0] + "|" + entries[1];
  page["subsection2"] = subsection;
  page["subsection3"] = subsection;  */
  page["pagecategory"] = "Checkout";
  checkBreadcrumbsAvailability();
  digitalData["page"] = page;
}
function trackPageCategoryData(pageName, pageCategory) {
    var entries = checkoutSignInPage.split("|");
    page["pageName"] = pageName;
    page["channel"] = entries[0] + "|" + entries[1];
    page["pagecategory"] = pageCategory.charAt(0).toUpperCase()+ pageCategory.slice(1).toLowerCase();
    
    checkBreadcrumbsAvailability();
    digitalData["page"] = page;
  }
//function for finding channel in page.
function trackChannelData(){
	  var breadcrumbs = [
	        "Home".toLowerCase(),
			  
				 "|Women".toLowerCase(),         
			  
				 "|Bags & Wallets".toLowerCase(),         
			  ]
			if (breadcrumbs.length == 1) {
			var pathName=window.location.pathname;
			if (pathName !== null && pathName!=="/") {
			breadcrumbs = [breadcrumbs + pathName.replace(/\//g, '|')]
			}
		}
		page["channel"]    =setBreadCrumbsData(2,breadcrumbs);
  }
 //function for finding Page Name and Page Category.
function trackPageRelatedData(pageName, pageCategory) {
    page["pageName"] = pageName;
    page["pagecategory"] = pageCategory.charAt(0).toUpperCase()+ pageCategory.slice(1).toLowerCase();
    digitalData["page"] = page;
  }
 
//function to capture non available sizes in PDP page.
 function trackNonCutSizes(){
	var nonCutSize=0;	
	var sizeDetails="";
	var totalSizes="";
	
	sizeDetails=(nonCutSize*100)/totalSizes+"%";
	if(sizeDetails != 'NaN%'){
	  product["sizeDetails"]=sizeDetails; 
	}
	_satellite.track('CPJ_PDP_Non_Cut_Size');
 }
	
//function for tracking cart entries or order entries skus.
function trackProductCodes(){
	if(false){	
		masterProduct = [
			                  ] 
		 colorVariant  = [
				            ] 
		sizeVariant  = [
					          ] 
		brandName  = [
					          ] 
		product[masterProductId] = masterProduct;
		product[colorVariantId] = colorVariant;
	    product[sizeVariantId] = sizeVariant;
	    product[brandId] = brandName;
	}else{
		masterProduct = [
					            ] 
		colorVariant = [
					            ] 
		sizeVariant = [
						        ] 
		brandName  = [
					          	] 
		product[masterProductId] = masterProduct;
	    product[colorVariantId] = colorVariant;
	    product[sizeVariantId] = sizeVariant;
	    product[brandId] = brandName;
	}
	    digitalData["product"] = product;
}

