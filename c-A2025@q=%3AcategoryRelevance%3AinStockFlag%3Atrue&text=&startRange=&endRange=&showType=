var firstTime=false
function GA_top_navigation_click(cta_name,eventName='',msiteFlag=true)
{
	
	
	if(eventName=="header_click")
	{
		
		eventName="header_click"
	
		// alert("ads")
	}
	else{
		// alert("ads")
		eventName="top_navigation_click"
		
	}
		var currentDevice = ""
		var phoneOrNot = !!navigator.maxTouchPoints;
		let phoneNum = "";
		
	if (phoneOrNot) {
		if (navigator.userAgent.match(/Android/i)) {
			currentDevice = "android"
		} else {
			currentDevice = "ios"
		}
	} else {
		currentDevice = "desktop"
	}
	dataLayer.push({
		ecommerce: null
	});
	var customer_type="NA"
	var user_type= "NA"
	var visitorType = "";

	if (sessionStorage.getItem("isLoggedIn") == "true") {
		visitorType = "loggedin"
		 user_type=localStorage.getItem("user_type_value") || "NA"
		 if(localStorage.getItem("user_type_value")){
	
	}
	} else {
		visitorType = "guest"
	}
	
		let prevpage="NA"
		var screenName="NA";
	
// alert(sessionStorage.getItem("prevPage"))
if(sessionStorage.getItem("prevPage")){
	let prevPageArray=JSON.parse(sessionStorage.getItem("prevPage"))
	if(prevPageArray.length>=1){
		screenName=prevPageArray[prevPageArray.length-1]
	}
	if(prevPageArray.length >=2){
		prevpage=prevPageArray[prevPageArray.length-2]
	}else if(prevPageArray.length ==1){
		prevpage="NA"
	}
}	
let userID="NA"
if (sessionStorage.getItem("isLoggedIn") == "true") {
	userID=localStorage.getItem("UserIdkey")
	
	} 
	// if(currentDevice != "desktop")
	// {

	
	// https://adminuat4.shopper-stop.in/my-account/isOrderPresent
	// $.ajax({
    //                   url : "/my-account/isOrderPresent",
    //                   type : "GET",
    //                   async: false,
    //                   success : function(res) {
				// if(res)
				// {
				// 	if (sessionStorage.getItem("isLoggedIn") == "true") {
				// 		customer_type="existing_customer"
				// 		}
				// 		else{
				// 			customer_type="NA"
				// 		}
					
				// }
				// else{
				// 		if (sessionStorage.getItem("isLoggedIn") == "true") {
				// 		customer_type="new_customer"
				// 		}
				// 		else{
				// 			customer_type="NA"
				// 		}

				// }
				var member_status="Non member"
				if(localStorage.getItem("GA_member_status")){
					if (sessionStorage.getItem("isLoggedIn") == "true") {
						member_status=localStorage.getItem("GA_member_status")
					}
				}
				// $.ajax({
                //       url : "/dy/customer/getUserDetails",
                //       type : "GET",
                //       async: false,
                //       success : function(data) {
						if(eventName=="header_click")
						{
								 if(!firstTime)
    							{
										dataLayer.push({'event':eventName,
										// 'user_type':user_type,
										// 'visitor_type':visitorType,
										// 'customer_type':customer_type,
										'userID':userID,
										'member_status':member_status , 
										'cta_name':cta_name.toLowerCase() ? cta_name.toLowerCase() :"NA",
										'screen_name':screenName,
										'previous_screen_name':prevpage
									});
									firstTime=true
								}
						}
						else{
							if(msiteFlag)
							{
						dataLayer.push({'event':eventName,
							// 'user_type':user_type,
							// 'visitor_type':visitorType,
							// 'customer_type':customer_type,
							'userID':userID,
							'member_status':member_status , 
							'cta_name':cta_name.toLowerCase() ? cta_name.toLowerCase() :"NA",
							'screen_name':screenName,
							'previous_screen_name':prevpage
						});
							}
						}
						// alert("Ads")
				// 	  }
				// })

	//   }})
	// }
}
function BargainsorLuxe(name,id)
{
	localStorage.setItem("BargainsorLuxe",name)
	localStorage.setItem("BargainsorLuxe_id",id)
}
			$(document).ready(function () {
var fistTime=0

				
 $(".l1cta > a").click(function(){
	
	// e.stopPropagation()

	// alert(JSON.stringify(this))
	// alert(	$(this).find("> a").prevObject[0].firstElementChild.innerText)
	// var newcta_name=$(this)?.find("> a")?.prevObject[0]?.firstElementChild?.innerText
	var newcta_name=$(this)[0]?.innerText
var currentDevice = ""
		var phoneOrNot = !!navigator.maxTouchPoints;
		let phoneNum = "";
		
	if (phoneOrNot) {
		if (navigator.userAgent.match(/Android/i)) {
			currentDevice = "android"
		} else {
			currentDevice = "ios"
		}
	} else {
		currentDevice = "desktop"
	}
	dataLayer.push({
		ecommerce: null
	});
	var customer_type="NA"
	var user_type= "NA"
	var visitorType = "";

	if (sessionStorage.getItem("isLoggedIn") == "true") {
		visitorType = "loggedin"
		 user_type=localStorage.getItem("user_type_value") || "NA"
		
	} else {
		visitorType = "guest"
	}
	
		let prevpage="NA"
		var screenName="NA";
	
// alert(sessionStorage.getItem("prevPage"))
if(sessionStorage.getItem("prevPage")){
	let prevPageArray=JSON.parse(sessionStorage.getItem("prevPage"))
	if(prevPageArray.length>=1){
		screenName=prevPageArray[prevPageArray.length-1]
	}
	if(prevPageArray.length >=2){
		prevpage=prevPageArray[prevPageArray.length-2]
	}else if(prevPageArray.length ==1){
		prevpage="NA"
	}
}	
let userID="NA"
if (sessionStorage.getItem("isLoggedIn") == "true") {
	userID=localStorage.getItem("UserIdkey")
	
	} else {
		// visitorType = "guest"
	}

// $.ajax({
//                       url : "/my-account/isOrderPresent",
//                       type : "GET",
//                       async: false,
//                       success : function(res) {
				
            $.ajax({
                      url : "/dy/customer/getUserDetails",
                      type : "GET",
                      async: false,
                      success : function(data) {
// alert("Dsf")
						if(currentDevice!= "desktop"){						
						if(!fistTime)
						{
							
								dataLayer.push({'event':'hamburger_interaction',
									// 	 'user_type':user_type,
									//  'visitor_type':visitorType,
									//  'customer_type':customer_type,
									'userID':userID,
									'member_status':data?.fccMembershipTier ? data?.fccMembershipTier :"Non member" ,
									// 'dropdown_option
									
									'cta_name':newcta_name.toLowerCase() || "NA",
									'screen_name':screenName,
									'previous_screen_name':prevpage
									});
									fistTime=1
									// alert(fistTime)
									setTimeout(() => {
										fistTime=0
									}, 1000);
							
						}
						
				}
					  }})
					//   }})




 
});
});
