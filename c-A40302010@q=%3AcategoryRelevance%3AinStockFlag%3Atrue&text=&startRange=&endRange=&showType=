	function sizeLocalStorage(size)
	{
		localStorage.setItem("forAddToBagSize",size)
	}
	var prevpage=""
// alert(sessionStorage.getItem("prevPage"))
if(sessionStorage.getItem("prevPage")){
	let prevPageArray=JSON.parse(sessionStorage.getItem("prevPage"))
	if(prevPageArray.length >=2){
		prevpage=prevPageArray[prevPageArray.length-2]
	}else if(prevPageArray.length ==1){
		prevpage=prevPageArray[prevPageArray.length-1]
	}
}
function setLocalForAddToCart(code,price,wasPrice,productURL)
{
	localStorage.setItem("forAddToBagPrice",price)
	localStorage.setItem("forAddToBagWasPrice",wasPrice)
	localStorage.setItem("forAddToBagCode",code)
	
	// localStorage.setItem("forAddToBagUrl",productURL)
	
}

function fromWishlist_addtobag(code,price,oldPrice,productURL)
{
	
	
			productDetailsUrl = "/cart/productBasicDetails";
			// var url = $(this).attr('href');
			$.ajax({
	            data : {
					'productCode' : code
				},
				type : 'Get',
				url : productDetailsUrl,
				success : function(res){
var t=res
// alert(JSON.stringify(res))
						$.ajax({
	type : "GET",
	url : window.location.origin+"/dy/customer/getL4Code?productCode="+code,
	success : function(response) {
		// alert("adsdsa")
		let wasPrice=0
		let curPrice=0
		// if(t?.originalPrice?.value)
		// {
		// 	wasPrice=t.originalPrice.value
		// }else if(t.price.value){
		// 	wasPrice=t.price.value
		// }
		// if(t?.price?.value)
		// {
		// 	curPrice=t.price.value
		// }
		// else if(t?.originalPrice?.value)
		// {
		// 	curPrice=t.originalPrice.value
		// }
		if(oldPrice)
		{
			// alert("sda")
			wasPrice=oldPrice
		}
		else{
			wasPrice=price
		}
		if(price)
		{
			curPrice=price
		}

		console.log(productURL,wasPrice,"data display")
		// alert(productURL)
		// alert(wasPrice)

		let categotys={	'category_id':response[0].l1Code,
		'category':response[0].l1Name,
		'categoryName':response[0].l1Name,
		'category2_id':response[0].l2Code,
		'category3_id':response[0].l3Code,
		'category4_id':response[0].l4Code ? response[0].l4Code:"" ,
		'category2':response[0].l2Name ? response[0].l2Name:"",
		'category3':response[0].l3Name ? response[0].l3Name:"",
		'category4':response[0].l4Name ? response[0].l4Name:"",
					'retail_price':parseFloat(curPrice.toFixed(2)),
					'price':wasPrice,
					'discount':parseFloat((wasPrice - curPrice).toFixed(2)) ,
					'path':productURL ? productURL : "ll"};
		if(t.sizeDesc)
		{
			categotys['item_size']=t.sizeDesc;
		}
	
		// alert("asda")
		// alert(JSON.stringify({event:"addToCart",ecommerce:{currencyCode:"INR",add:{products:[{name:t.name,id:code,price:wasPrice,brand:t.brandCode,category:t.primaryCategoryCode,categoryName:i,variant:t.baseOptions[0].selected.variantOptionQualifiers[0].value,quantity:1,image:[Images[0]],...categotys}]}}}))
		dataLayer.push({event:"addToCart",ecommerce:{currencyCode:"INR",add:{products:[{name:t.name,id:code,price:parseFloat(wasPrice.toFixed()),brand:t.brandCode,category:t.primaryCategoryCode,categoryName:i,variant:t.baseOptions[0].selected.variantOptionQualifiers[0].value,quantity:1,image:[Images[0]],...categotys}]}}})
		// alert(categotys)
			// localStorage.removeItem("forAddToBagCode")
	},
	error : function(xhr, status, error) {
		console.log(error)
		// alert(error);
	
	}
});










					// alert(JSON.stringify(response))
					// console.log(response)
				},
				error : function(jqXHR, textStatus, errorThrown) {
					console.log(JSON.stringify(jqXHR));
					console.log("AJAX error: " + textStatus + ' : ' + errorThrown);
				},
                // complete: function () {
                //     // window.location.href = url;
                // }
			});
}


//////////////////////////////
	function setStorage()
	{
		// alert("ASd")
	localStorage.setItem("setStorageLocal",1);

	}
	$(document).ready(function() {
		
	
	
	if(localStorage.getItem("setStorageLocal"))
	{
		// alert("dsa")
		localStorage.removeItem("setStorageLocal")
		$.ajax({
			type : "GET",
			url : window.location.origin+"/dy/customer/getWishListData",
			success : function(response) {
				console.log(response);
            	console.log("wishlist data");
				let priceOfProds=0;
				for(let i=0;i<response.length;i++)
				{
						priceOfProds=priceOfProds+response[i].price.value
				}
				console.log(priceOfProds)
				let prodDetails=[]
				for(let i=0;i<response.length;i++)
				{
					let wasPrice=0
					let curPrice=0
					if(response[i].price)
					{
						wasPrice=response[i].price.value
					}else if(response[i].price){
						wasPrice=response[i].price.value
					}
					if(response[i].price)
					{
						curPrice=response[i].price.value
					}
					else if(response[i].originalPrice)
					{
						curPrice=response[i].originalPrice.value
					}
					prodDetails.push({
						brand:response[i].brandName,
						brand_id:response[i].brandCode,
						price:parseFloat(wasPrice.toFixed(2)),
						image:[response[i].images ? response[i].images[3].url :""],
						name:response[i].name,
						id:response[i].code,
						discount:parseFloat((wasPrice-curPrice).toFixed(2)),
						retail_price:parseFloat(curPrice.toFixed(2))

					})	
				}
			if(sessionStorage.getItem("isLoggedIn")=="true")
			{		
							dataLayer.push({
								"event":"wishlistUpdated",
								"ecommerce":{
								"currencyCode":"INR",
								"wishlist-updated":{
								"no_of_items":response.length,
								"total_amount":priceOfProds,
								"products":prodDetails
								}
							}
							})
			}
			},
			error : function(xhr, status, error) {
				
				console.log(error)
			
			
			}
		});

	}
})
	function wishlistAdd(productcode)
	{
		// alert("dsasda")
		let eventName=""
		if(document.getElementById("test_"+productcode).classList.contains("active"))
		{
			eventName = 'remove_to_wishlist';
		}
		else{
			eventName = 'add_to_wishlist';
		}
		// if(test_)
		localStorage.setItem("wshListInd",1)
		console.log("inside wishlistAdd")
		//  value="S24B101211_BLUE;Solid Cotton Skinny Fit Girls Jeans - Blue;TALES & STORIES;;2199.0;791.64"
	let data=document.getElementsByClassName(productcode)[0].value.split(";");
	
	console.log(data)
			$.ajax({
			type : "GET",
			url : window.location.origin+"/dy/customer/getL4Code?productCode="+data[0],
			success : function(response) {
				console.log(response);
				if(sessionStorage.getItem("isLoggedIn") =="true")
				{

					let variantColor=""
					if(data[0].split("_").slice(1)[0])
					{
						variantColor=data[0].split("_").slice(1)[0]
					}
					if(data[0].split("_").slice(1)[0])
					{
						variantColor=variantColor+data[0].split("_").slice(1)[1]	
					}

					let wasPrice=0
					let curPrice=0
					if(parseFloat(data[4]))
					{
						wasPrice=parseFloat(parseFloat(data[4]).toFixed(2))
					}else if(parseFloat(data[5])){
						wasPrice=parseFloat(parseFloat(data[5]).toFixed(2))
					}
					if(parseFloat(data[5]))
					{
						curPrice=parseFloat(parseFloat(data[5]).toFixed(2))
					}
					else if(parseFloat(data[4]))
					{
						curPrice=parseFloat(parseFloat(data[4]).toFixed(2))
					}
console.log( wasPrice+ curPrice + "previous and current price")

						dataLayer.push({
			  'event':eventName,
			  'ecommerce': {
			    'currencyCode': 'INR',
			    'wishList': {                              
			      'products': [{                      
			        'name': data[1],
			        'id': data[0], //Product ID
			 		'itemCategory': '',
			 		    'brand':data[2],
				 	'variant': data[0].split("_").slice(1).length ? data[0].split("_").slice(1)[0] :"",
			        'quantity':1,					
				'category_id':response[0].l1Code,
				'categoryName' :response[0].l1Name,	 
				'category':response[0].l1Name,
				'category2_id':response[0].l2Code,
				'category3_id':response[0].l3Code,
				'category4_id':response[0].l4Code ? response[0].l4Code:"" ,
				'category2':response[0].l2Name ? response[0].l2Name:"",
				'category3':response[0].l3Name ? response[0].l3Name:"",
				'category4':response[0].l4Name ? response[0].l4Name:"",
					'path':window.location.href,
					'retail_price':parseFloat(parseFloat(data[5]).toFixed(2)) || parseFloat(parseFloat(data[4]).toFixed(2)),
					'price':parseFloat(parseFloat(data[4]).toFixed(2)) || parseFloat(parseFloat(data[5]).toFixed(2)),            
					'discount': (wasPrice-curPrice).toFixed(2) ? (wasPrice-curPrice).toFixed(2) :0,
					'image':[data[7]]
				// 	// 'discount':parseFloat("64"),
				// 'image':[imagesList[0]]
			       }]
			    }
			  }
			});


			 dataLayer.push({
    ecommerce: null
});

if(eventName == 'add_to_wishlist')
{
   var currentDevice = ""
                                           var phoneOrNot = !!navigator.maxTouchPoints;
                                            let phoneNum = "";

                                            if (phoneOrNot) {
                                                if (navigator.userAgent.match(/Android/i)) {
                                                    currentDevice = "android"
                                                } else {
                                                    currentDevice = "ios"
                                                }
                                            } else {
                                                currentDevice = "desktop"
                                            }

                                            dataLayer.push({
                                                ecommerce: null
                                            });
                                            var visitorType = "";
                                            if (sessionStorage.getItem("isLoggedIn") == "true") {
                                                visitorType = "loggedin"
                                            } else {
                                                visitorType = "guest"
                                            }
                                            if (sessionStorage.getItem("isLoggedIn") == "true") {
                                                phoneNum = "+91" + localStorage.getItem("userNumber")
                                            } else {
                                                phoneNum = "NA"

                                            }
	let userID="NA"
								 if (sessionStorage.getItem("isLoggedIn") == "true") {
									userID=localStorage.getItem("UserIdkey")
									
									} else {
										// visitorType = "guest"
									}
	$.ajax({
			type : "GET",
			url : window.location.origin+"/dy/customer/getCOO?ProductCode="+data[0],
			success : function(responsecoo) {
dataLayer.push({
    'event': 'ga_add_to_wishlist',
    'userID': userID ,
    'screen_name': 'product listing page',
    'previous_screen_name': prevpage,
    'visitor_type': visitorType,
    'platform': currentDevice,
	'value': data[5] ? parseFloat(parseFloat(data[5]).toFixed(2)) : parseFloat(parseFloat(data[6]).toFixed(2)),
	'pagepath': window.location.pathname,
    'ecommerce': {
        'currency': 'INR',
        'value':data[5] ? parseFloat(parseFloat(data[5]).toFixed(2)) : parseFloat(parseFloat(data[6]).toFixed(2)),
        'items': [{
            'item_name':  data[1],
            'item_id': data[0],
            'item_brand': data[2],
            'item_variant':"NA",
            'item_category': (response[0].l1Name ? response[0].l1Name:"NA")  ,
            // 'item_category':( response[0].l1Name ? response[0].l1Name:"NA") ,
			'item_category2': ( response[0].l2Name ? response[0].l2Name:"NA"),
			'item_category3':( response[0].l3Name ? response[0].l3Name:"NA"),
			'item_category4':"NA",
            'item_category5': 'NA',
            'currency': 'INR',
            'discount':  parseInt((wasPrice-curPrice).toFixed(2)) ? parseInt((wasPrice-curPrice).toFixed(2)) :"NA",
            'price': data[5] ? parseFloat(parseFloat(data[5]).toFixed(2)) : parseFloat(parseFloat(data[6]).toFixed(2)),
            'quantity': "1",
            'index': 1
        }]
    }
});
}})
}


				}
			
			
			}
			})
	}
