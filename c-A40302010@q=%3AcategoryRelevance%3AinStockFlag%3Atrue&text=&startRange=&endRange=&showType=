function GA_hamburger_interaction(cta_name=""){
	// alert("ADS")
var currentDevice = ""
		var phoneOrNot = !!navigator.maxTouchPoints;
		let phoneNum = "";
		
	if (phoneOrNot) {
		if (navigator.userAgent.match(/Android/i)) {
			currentDevice = "android"
		} else {
			currentDevice = "ios"
		}
	} else {
		currentDevice = "desktop"
	}
	dataLayer.push({
		ecommerce: null
	});
	var customer_type="NA"
	var user_type= "NA"
	var visitorType = "";

	if (sessionStorage.getItem("isLoggedIn") == "true") {
		visitorType = "loggedin"
		 user_type=localStorage.getItem("user_type_value") || "NA"
		 if(localStorage.getItem("user_type_value")){
	
	}
	} else {
		visitorType = "guest"
	}
	
		let prevpage="NA"
		var screenName="NA";
	
// alert(sessionStorage.getItem("prevPage"))
if(sessionStorage.getItem("prevPage")){
	let prevPageArray=JSON.parse(sessionStorage.getItem("prevPage"))
	if(prevPageArray.length>=1){
		screenName=prevPageArray[prevPageArray.length-1]
	}
	if(prevPageArray.length >=2){
		prevpage=prevPageArray[prevPageArray.length-2]
	}else if(prevPageArray.length ==1){
		prevpage="NA"
	}
}	
let userID="NA"
if (sessionStorage.getItem("isLoggedIn") == "true") {
	userID=localStorage.getItem("UserIdkey")
	
	} else {
		// visitorType = "guest"
	}

// $.ajax({
//                       url : "/my-account/isOrderPresent",
//                       type : "GET",
//                       async: false,
//                       success : function(res) {
				// if(res)
				// {
				// 	if (sessionStorage.getItem("isLoggedIn") == "true") {
				// 		customer_type="existing_customer"
				// 		}
				// 		else{
				// 			customer_type="NA"
				// 		}
					
				// }
				// else{
				// 		if (sessionStorage.getItem("isLoggedIn") == "true") {
				// 		customer_type="new_customer"
				// 		}
				// 		else{
				// 			customer_type="NA"
				// 		}

				// }
				$.ajax({
                      url : "/dy/customer/getUserDetails",
                      type : "GET",
                      async: false,
                      success : function(data) {		
						if(currentDevice != "desktop")
						{
							dataLayer.push({'event':'hamburger_interaction',
							// 'user_type':user_type,
							// 'visitor_type':visitorType,
							// 'customer_type':customer_type,
							'userID':userID,
							'member_status':data?.fccMembershipTier ? data?.fccMembershipTier :"Non member" , 
							'cta_name':cta_name.toLowerCase(),
							'screen_name':screenName,
							'previous_screen_name':prevpage
							});
						}
						// alert("Ads")
					  }
				})

	//   }})



