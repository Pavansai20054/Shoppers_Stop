/**
 * to track the store selction /customer information page 
 */
function trackMacOrPersonalShopperPages(){
	if(typeof "" != 'undefined' && "" != null && "" != '' && "" != 'undefined' && "" != null && "" != '' ){
		if("" == '798330001'){
			page["pageName"]='PS Store Selection Page';
			trackPSCategory();
			_satellite.track('ps_store_selection_page');
			return true;
		}else if("" == '798330002'){
			page["pageName"]='Mac Store Selection Page';
			trackMacServiceCategory();
			_satellite.track('mac_store_selection_page');
			return true;
		}
		return false;
}else if(typeof "" != 'undefined' && ""  != null && "" != '' && "" != 'undefined' && "" != null && "" != '' ){
		if("" == '798330001'){
			page["pageName"]='PS Customer Information Page';
			trackPSCategory();
			_satellite.track('ps_customer_information_page');
			return true;
		}else if("" == '798330002'){
			page["pageName"]='Mac Customer Information Page';
			trackMacServiceCategory();
			_satellite.track('mac_customer_information_page');
			return true;
		}
		return false;
}
	return false;
}

//personal shopper category tracking
function trackPSCategory(){
	page["pagecategory"] = "Personal Shopper";
}

//mac  category tracking
function trackMacServiceCategory(){
	page["pagecategory"] = "Beauty Service";
}

//to track the payment modes
function trackPaymentMode(paymentmode){
	if(paymentmode == 'COD Payment'){
		return "COD";
	}else if(paymentmode == 'credit card'){
		return 'CREDIT CARD';
	}else if(paymentmode == 'debit card'){
		return 'DEBIT CARD';
	}else if(paymentmode == 'debit card'){
		return 'DEBIT CARD';
	}else if(paymentmode == 'ss credits payment'){
		return 'SS WALLET';	
	}else if(paymentmode == 'Paytm Payment Gateway'){
		return 'PAYTM';	
	}else if(paymentmode == 'gift caed / e-gift voucher'){
		return 'GIFT CARD / E-GIFT VOUCHER';	
	}else if(paymentmode == 'CASH ON DELIVERY'){
		return "COD";
	}else{
		return paymentmode;
	}
 }


</script><script type="text/javascript">

var pageType=document.getElementById("pageType").value;

function trackDYLogin()
{
    localStorage.setItem('loginEvent', 'true')
}

function trackDYRegistration(email,gender){
  if (typeof DY.API === "function") {
  
     if(typeof email !== "undefined" && email !== ""){
         DY.API("event", {
                 name: "Signup",
                 properties: {
                     dyType: "signup-v1",
                     hashedEmail: DYO.dyhash.sha256(email),
                     metadata: {
                     gender : gender
                 }
             }
         });
         trackDYIdentifyAPI(email);
     }
  }
}


function trackDYPromoCodeEntered(){
 
 if (typeof DY.API === "function") {


     var orderLevelPromotions = [
                    	               ];
    $.each(orderLevelPromotions, function(){
    	if(typeof this.valueOf() !== "undefined"){
            DY.API("event", {
                name: "Promo Code Entered",
                properties: {
                    dyType: "enter-promo-code-v1",
                    code: this.valueOf()
                    }
                });
        }
    });
 }
 
}

function trackDYNewsletterSubscription(email){
    if (typeof DY.API === "function") {
        DY.API("event", {
            name: "Newsletter Subscription",
            properties: {
                dyType: "newsletter-subscription-v1",
                hashedEmail: DYO.dyhash.sha256(email)
            }
        });
        trackDYIdentifyAPI(email);
    }
}


if(pageType=='PRODUCTSEARCH' || pageType=='CATEGORY' || pageType == 'CATEGORYBRAND' ){
    if (typeof DY.API === "function") {
        var sortItem = $("#sort-by-list").find(".active").attr("sortCode"), sortBy, sortOrder;
        var map = new Object();
        map["categoryRelevance"] = "RELEVANCE,ASC";
        map["topRated"] = "POPULARITY,ASC";
        map["newArrival-desc"] = "NEW,DESC";
        map["discount-desc"] = "DISCOUNT,DESC";
        map["bestSeller-desc"] = "BESTSELLER,DESC";
        map["price-asc"] = "PRICE,ASC";
        map["price-desc"] = "PRICE,DESC";
        if (typeof sortItem !== 'undefined') {
            if (typeof map[sortItem] == 'undefined' || typeof map[sortItem] == '') {
                sortOrder = "ASC";
                sortBy: "OTHER";
            } else {
                sortBy = map[sortItem].split(',')[0];
                sortOrder = map[sortItem].split(',')[1];
            }
            DY.API("event", {
                name: "Sort Items",
                properties: {
                    dyType: "sort-items-v1",
                    sortBy: sortBy,
                    sortOrder: sortOrder,
                }
            });
        }
    }
}

if(pageType=='PRODUCTSEARCH'){
    if (typeof DY.API === "function") {
        if (typeof search.term != 'undefined') {
            DY.API("event", {
                name: "Keyword Search",
                properties: {
                    dyType: "keyword-search-v1",
                    keywords: search.term
                }
            });
        }
    }
}


$(document).on('click','.filter-inner .ex-checkbox label',function(e){
    if (typeof DY.API === "function") {
        var facet = $(this).attr('for');
        var facetCategory = $(this).closest(".filter-wpr").find(".filter-category-heading").text().trim().split('\t')[0].trim()
        if ($(this).siblings('.facet-checkbox').prop('checked')) {
            return;
        } else {
            DY.API("event", {
                name: "Filter Items",
                properties: {
                    dyType: "filter-items-v1",
                    filterType: facetCategory,
                    filterStringValue: facet
                }
            });
        }
    }
});

if(pageType=='ORDERCONFIRMATION'){
    if (typeof DY.API === "function") {
        var orderId = 'null';
	if(orderId != ''){
    	trackDYPromoCodeEntered();
        if($('[data-id="isFccProductPurchase"]').val()=="true" || $('[data-id="isFccProductPurchase"]').val()==true){
        	trackDYFirstCitizenPurchase();
        }
        DY.API("event", {
            name: "Purchase",
            properties: {
                uniqueTransactionId: "",
                
                dyType: "purchase-v1",
                value: parseFloat(),
                currency: "INR",
                cart: [
                    
                ]
            }
        });
	}
    }
}

if(pageType=='CART'){

    if (typeof DY.API === "function") {

        var cartAttributes = {
          
        };
    if(cartAttributes.cartOperation== "REMOVE" && typeof cartAttributes.code !== "undefined" && cartAttributes.code !== ""){
        DY.API("event", {
            name: "Remove from Cart",
            properties: {
                dyType: "remove-from-cart-v1",
                value: parseFloat(cartAttributes.price),
                currency: "INR",
                productId: cartAttributes.code,
                quantity: parseInt(cartAttributes.quantity),
                size: cartAttributes.size,
                cart: [
                    
                ]
            }
        });
    }
    else if(cartAttributes.cartOperation == "UPDATE"){
        DY.API("event", {
            name: "Sync cart",
            properties: {
                dyType: "sync-cart-v1",
                currency: "INR",
                cart: [
                    
                ]
            }
        });
    }
    }
}

function trackDYAddtoWishlist(productId){

    if (typeof DY.API === "function") {
        DY.API("event", {
            name: "Add to Wishlist",
            properties: {
                dyType: "add-to-wishlist-v1",
                productId: productId,
            }
        });
    }
}
function trackRemoveFromWishlist(productId){

    if (typeof DY.API === "function") {
        DY.API("event", {
            name: "Remove from Wishlist",
            properties: {
                dyType: "remove-from-wishlist-v1",
                productId: productId,
            }
        });
    }
}

$(document).on('click','.productAction .wishlist-acn .add-to-wishlist',function(e){
    if (typeof DY.API === "function") {
        var productId = $(this).closest('li.clearfix').find('.pro-img').find('a:first-child').attr('href').split("colorCode=");

        DY.API("event", {
            name: "Add to Wishlist",
            properties: {
                dyType: "add-to-wishlist-v1",
                productId: productId[1],
            }
        });
    }
 });

function trackDYAddtoCart(form,data){

    if (typeof DY.API === "function") {

        var variantColorCode = $(form).find('[data-id="styleCode"]').val();
        var size = $(form).find('[data-id="sizeValue"]').val();
        var price = $(form).find('[data-id="price"]').val();
        var jsonObj = [];
        $.each(data.entries, function () {
            jsonObj.push({
                'productId': this.product.styleCode,
                'quantity': parseInt(this.quantity),
                'itemPrice': this.product.price.value,
            });
        });
        if (typeof variantColorCode !== 'undefined' && variantColorCode !== "") {
            DY.API("event", {
                name: "Add to Cart",
                properties: {
                    dyType: "add-to-cart-v1",
                    value: parseFloat(price),
                    currency: "INR",
                    productId: variantColorCode,
                    quantity: 1,
                    size: size,
                    cart: jsonObj
                }
            });
        }
    }

}

if (localStorage.getItem("loginEvent") == "true") {
    if (typeof DY.API === "function") {
        var email = (typeof ACC.global === "undefined" || typeof ACC.global === undefined) ? "anonymous" : ACC.global.getUserData().userId;
        DY.API("event", {
            name: "Login",
            properties: {
                dyType: "login-v1",
                hashedEmail: DYO.dyhash.sha256(email),
            }
        });
        trackDYFCCLogin();
        trackDYIdentifyAPI(email);
        localStorage.removeItem("loginEvent");
    }
}

function trackDYPersonalShopperBooking() {
    if (typeof DY.API === "function") {
        DY.API("event", {
            name: "Personal Shopper Booking",
            properties: {
                type: "custom"
            }
        });
    }
}

function trackDYFirstCitizenPurchase() {
    if (typeof DY.API === "function") {
        DY.API("event", {
            name: "First Citizen Purchase",
            properties: {
                type: "custom"
            }
        });
    }
}

function trackDYFCCLogin() {
    if (typeof DY.API === "function") {
        var isFCCLinked = (typeof ACC.global === "undefined" || typeof ACC.global === undefined) ? "false" : ACC.global.getUserData().isFCClinked;
        if (typeof isFCCLinked !== undefined && isFCCLinked == 'true') {
            DY.API("event", {
                name: "First Citizen Login",
                properties: {
                    type: "custom"
                }
            });
        }
    }
}

function trackDYFCCLink() {
    if (typeof DY.API === "function") {
        DY.API("event", {
            name: "First Citizen Card Link",
            properties: {
                type: "custom"
            }
        });
    }
}

function trackDYIdentifyAPI(email) {
    if (typeof DY.API === "function") {
        DY.API("identify", {
            uid: DYO.dyhash.sha256(email), //SHA 256 hashed email of the plain text email in lower case
            type: "he"
        });
    }
}
</script><script type="text/javascript" defer>

    var pageType = document.getElementById("pageType").value;
    var retryOsCount=0;
	function deferFunCall(osFun,hyFun) {
		retryOsCount++;
	    if(typeof osFun === "function") {
	    	hyFun();
	    } else if(retryOsCount<=5) {
	    	window.setTimeout(deferFunCall(osFun,hyFun), 100); /* this checks the flag every 100 milliseconds*/
	    }else{
	    	console.log(osFun + " not found!!");
	    }
	}

	function sslOSViewProduct() {
		if (typeof _osViewProduct === "function") {
            var category = "";
            category = category.replace("[", "").replace(/, /gi, ' > ').replace("]", "").replace("All Products > ", "");
            var discount;
            
            _osViewProduct({
                "products": [{
                    "skuId": "",
                    "sellerId": "",
                    "category": category,
                    "productPrice": parseFloat(),
                    "currency": "",
                    "discount": discount
                }]
            })

        }
	}
	
    if (pageType === 'PRODUCT') {
    	deferFunCall(_osViewProduct, sslOSViewProduct);
    }

    function extractedOsCheckout() {
        if (typeof _osCheckout === "function") {
            var products = [
                
            ];
            _osCheckout({
                products,
                "totalAmount": "",
                "currency": ""
            })
        }
    }

    // Handling quickbuy case with pagetype and document last url should be pdp.
    if (pageType === 'CHECKOUT' && document.referrer.indexOf("/p-") !== -1) {
    	deferFunCall(_osCheckout, extractedOsCheckout);
    }

    $(document).on('click', '.btn-place-order', function (e) {
        extractedOsCheckout();
    });
    
    function sslOSSalesComplete() {
    	if (typeof _osSaleComplete === "function") {
            var orderId = 'null';
            if (orderId !== '') {
                var order = [
                    
                ];

                _osSaleComplete({
                    "products": order,
                    "totalAmount": parseFloat(),
                    "currency": "",
                    "orderId": "",
                    
                    "paymentMethod": ""
                })
            }
        }
    }

    if (pageType === 'ORDERCONFIRMATION') {
		deferFunCall(_osSaleComplete, sslOSSalesComplete);
    }

    function sslOSViewCategory() {
    	if (typeof _osViewCategory === "function") {
            var categoryList = "Kids > Girls > Bottomwear > Jeans & Jeggings";
            $(document).ready(function () {
                var sortItem = $("#sortMobileForm1").find("#ss1 option:selected").val(), sortBy, sortOrder;
                var map = {};
                var brand = [];
                var price = [];
                var color = [];
                map["categoryRelevance"] = "categoryRelevance,de";
                map["topRated"] = "topRated,de";
                map["newArrival-desc"] = "newArrival,de";
                map["discount-desc"] = "discount,de";
                map["discount-asc"] = "discount,as";
                map["bestSeller-desc"] = "bestSeller,de";
                map["bestSeller-asc"] = "bestSeller,as";
                map["price-asc"] = "price,as";
                map["price-desc"] = "price,de";
                $.each($("input[class='facet-checkbox-mobile']:checked"), function () {
                    var facetValue = $(this).attr("data-facet-value");
                    var facetCode = $(this).attr("data-facet-heading");
                    if (facetCode === 'brands') {
                        brand.push(facetValue);
                    } else if (facetCode === 'price') {
                        facetValue.split("-").forEach(item => price.push(item));
                    } else if (facetCode === 'swatchColors') {
                        color.push(facetValue);
                    }
                });
                
                if (price.length > 0) {
                    price = price.sort(function (a, b) {
                        return a - b;
                    });
                }
                if (typeof sortItem !== 'undefined') {
                    if (typeof map[sortItem] == 'undefined') {
                        sortOrder = "as";
                        sortBy = "other";
                    } else {
                        sortBy = map[sortItem].split(',')[0];
                        sortOrder = map[sortItem].split(',')[1];
                    }
                }
                _osViewCategory({
                    "name": categoryList,
                    "minPrice": price[0],
                    "maxPrice": price[price.length - 1],
                    "sortKey": sortBy,
                    "sort": sortOrder,
                    "brands": brand,
                    "colours": color
                })
            });
        }
    }
    
    if (pageType === 'CATEGORY' || pageType === 'CATEGORYBRAND' || pageType === 'PRODUCTSEARCH') {
    	deferFunCall(_osViewCategory, sslOSViewCategory);
    }
    
    function sslOSViewL1Category() {
    	if (typeof _osViewCategory === "function") {
            _osViewCategory({
                "name": "Jeans & Jeggings",
            })
        }
    }

    if (pageType === 'CATEGORYLANDING') {
    	deferFunCall(_osViewCategory, sslOSViewL1Category);
    }

