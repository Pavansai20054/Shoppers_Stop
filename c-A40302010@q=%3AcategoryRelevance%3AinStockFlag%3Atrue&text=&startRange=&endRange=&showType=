    	    			product.discount =Math.abs(eval(1699.0 - 985.42))
	                
    	   			wishlistedProducts.push(product)
    	   			
    	   		if(wishlistedProducts.length > 0 ){
    	    	var product = wishlistedProducts[0];
        		for (index = 0; index < wishlistedProducts.length; index++) {
        			var product = wishlistedProducts[index];
        		    if(product.code == selectedProductCode){
        		    	break;
        		    }
        		}
        		
                product.category = product.category.replace("[", "").replace(/, /gi, ' > ').replace("]", "").replace("All Products > ", "");
                var discount = product.discount;
        		 _osAdd2Cart({
                     "products": [{
                         "skuId": product.code,
                         "sellerId": product.brandCode,
                         "category": product.category,
                         "productPrice":product.price ,
                         "currency": product.currencyIso,
                         "discount": product.discount
                     }]
                 })
    	   		}
       		}
        	
    }
    }

    function trackOSViewCart() {
        if (typeof _osViewCart === "function") {
            if (pageType === 'CART') {
                
            }
        }
    }
    
    if (pageType === 'CART') {
    	deferFunCall(_osViewCart, trackOSViewCart);
    }
