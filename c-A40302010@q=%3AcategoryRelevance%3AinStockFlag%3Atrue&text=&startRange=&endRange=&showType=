// alert("sdffds")
	 onPromoClick(promoObj);
});
var firstTime=0
function onPromoClick(promoObj) {
	var currentPage="NA"
	var prevpage="NA"
	if(sessionStorage.getItem("prevPage")){
	let prevPageArray=JSON.parse(sessionStorage.getItem("prevPage"))
	if(prevPageArray.length >=2){
		prevpage=prevPageArray[prevPageArray.length-2]
	}else if(prevPageArray.length ==1){
		prevpage="NA"
	}
}
if(sessionStorage.getItem("prevPage")){
	let prevPageArray=JSON.parse(sessionStorage.getItem("prevPage"))
	if(prevPageArray.length >=1){
		currentPage=prevPageArray[prevPageArray.length-1]
	}else if(prevPageArray.length ==1){
		currentPage="NA"
	}
}

  var currentDevice = ""
var phoneOrNot = !!navigator.maxTouchPoints;
let phoneNum = "";

if (phoneOrNot) {
	if (navigator.userAgent.match(/Android/i)) {
		currentDevice = "android"
	} else {
		currentDevice = "ios"
	}
} else {
	currentDevice = "desktop"
}

                                          
var visitorType = "";
if (sessionStorage.getItem("isLoggedIn") == "true") {
	visitorType = "loggedin"
} else {
	visitorType = "guest"
}

                                    dataLayer.push({
                                        ecommerce: null
                                    });
								let userID="NA"
								 if (sessionStorage.getItem("isLoggedIn") == "true") {
									userID=localStorage.getItem("UserIdkey")
									
									} 
				
if(!firstTime)
{	
					dataLayer.push({
	'event': 'select_promotion',
	'userID': userID, 
	'screen_name':currentPage, 
	'previous_screen_name': prevpage, 
	'visitor_type': visitorType, 
	'platform': currentDevice, 
	'pagepath': window.location.pathname, 
	'ecommerce': {
		'items': [
		{
		'promotion_id': promoObj.id, 
		'promotion_name':promoObj.name, 
		'creative_name': promoObj.creative, 
		'creative_slot':parseInt(promoObj.pos)+1, 
		}]
	}
	});
	firstTime=1
}					
	

	  dataLayer.push({
	    'event': 'promotionClick',
	    'ecommerce': {
	      'promoClick': {
	        'promotions': [
	         {
	           'id': promoObj.id,                         // Name or ID is required.
	           'name': promoObj.name,
	           'creative': promoObj.creative,
	           'position': promoObj.pos,
			   'pageType': `CATEGORY`
	         }]
	      }
	    },
	    'eventCallback': function() {
	      document.location = promoObj.destinationUrl;
	    }
	  });
	}

