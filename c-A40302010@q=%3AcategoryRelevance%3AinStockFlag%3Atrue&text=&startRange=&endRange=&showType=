	//Product Click Event
	
	$(document).on('click','#qv-drop li.pro-box', function(e){
		var clickedProductDetailsUrl = "/cart/productBasicDetails";
		productPosition = $(this).find('.nApPostion').val(); 
	   $.ajax({
		    data : {
		     'productCode' : $(this).attr('data-product-code')
		    },
		    type : 'Get',
		    url : clickedProductDetailsUrl,
		    success : HomeObject,
		    error : function(jqXHR, textStatus, errorThrown) {
		     console.log(JSON.stringify(jqXHR));
		     console.log("AJAX error: " + textStatus + ' : ' + errorThrown);
		    }
	   });
	   
			   function HomeObject(productObj) {

				$.ajax({
			type : "GET",
			url : window.location.origin+"/dy/customer/getWishListData",
			success : function(response) {
		console.log(response);
            	console.log("wishlist data");
				let priceOfProds=0;
				for(let i=0;i<response.length;i++)
				{
						priceOfProds=priceOfProds+response[i].price.value
				}
				console.log(priceOfProds)
				let prodDetails=[]
				for(let i=0;i<response.length;i++)
				{
					let wasPrice=0
					let curPrice=0
					if(response[i].price)
					{
						wasPrice=response[i].price.value
					}else if(response[i].price){
						wasPrice=response[i].price.value
					}
					if(response[i].price)
					{
						curPrice=response[i].price.value
					}
					else if(response[i].originalPrice)
					{
						curPrice=response[i].originalPrice.value
					}
					prodDetails.push({
						brand:response[i].brandName,
						brand_id:response[i].brandCode,
						price:parseFloat(wasPrice.toFixed(2)),
						image:[response[i].images ? response[i].images[3].url :""],
						name:response[i].name,
						id:response[i].code,
						discount:parseFloat((wasPrice-curPrice).toFixed(2)),
						retail_price:parseFloat(curPrice.toFixed(2))

					})	
				}
			if(sessionStorage.getItem("isLoggedIn")=="true")
			{		
				if(localStorage.getItem("wshListInd"))
				{
					dataLayer.push({
								"event":"wishlistUpdated",
								"ecommerce":{
								"currencyCode":"INR",
								"wishlist-updated":{
								"no_of_items":response.length,
								"total_amount":priceOfProds,
								"products":prodDetails
								}
							}
							})
						localStorage.removeItem("wshListInd")

				}
				
			}
			},
			error : function(xhr, status, error) {
				
				console.log(error)
			
			
			}
		});
				   var productPrice = 0;
				   console.log("sadsadsda")
				 if(productObj.baseOptions[0].selected.priceData){
					productPrice= productObj.baseOptions[0].selected.priceData.value;
				 }
				 var itemCategoryName;
					productObj.categories.forEach((element) => {
						if(element.code === productObj.primaryCategoryCode)
						{itemCategoryName = element.name}
					})
					// alert("abcd")
								dataLayer.push({ ecommerce: null }); 
				var currentDevice = ""
		var phoneOrNot = !!navigator.maxTouchPoints;
	let phoneNum = "";

	if (phoneOrNot) {
		if (navigator.userAgent.match(/Android/i)) {
			currentDevice = "android"
		} else {
			currentDevice = "ios"
		}
	} else {
		currentDevice = "desktop"
	}

	dataLayer.push({
		ecommerce: null
	});
	var visitorType = "";
	if (sessionStorage.getItem("isLoggedIn") == "true") {
		visitorType = "loggedin"
	} else {
		visitorType = "guest"
	}
	if (sessionStorage.getItem("isLoggedIn") == "true") {
		phoneNum = "+91" + localStorage.getItem("userNumber")
	} else {
		phoneNum = "NA"

	}
		let prevpage="NA"
// alert(sessionStorage.getItem("prevPage"))
if(sessionStorage.getItem("prevPage")){
	let prevPageArray=JSON.parse(sessionStorage.getItem("prevPage"))
	if(prevPageArray.length >=2){
		prevpage=prevPageArray[prevPageArray.length-2]
	}else if(prevPageArray.length ==1){
		prevpage="NA"
	}
}	
let userID="NA"
if (sessionStorage.getItem("isLoggedIn") == "true") {
	userID=localStorage.getItem("UserIdkey")
	
	} else {
		// visitorType = "guest"
	}
	
	$.ajax({
			type : "GET",
			url : window.location.origin+"/dy/customer/getL4Code?productCode="+productObj.code,
			success : function(response) {
      
      dataLayer.push({
  'event': 'select_item',
  'userID': userID,
 'screen_name': 'product listing page',
 'previous_screen_name': prevpage, 
 'visitor_type':visitorType,
 'platform': currentDevice, 
 'pagepath': window.location.pathname,
  'ecommerce': {
    'items': [
     {    
       'item_name': productObj.name, 
       'item_id': productObj.code,
       'item_brand':productObj.brandCode,
       'currency': 'INR', 
      'discount': "NA",
       'item_variant': productObj.baseOptions[0].selected.variantOptionQualifiers[0].value,
		'item_category': response[0].l1Name , 
       'item_category2': response[0].l2Name, 
       'item_category3': response[0].l3Name ? response[0].l3Name :"NA",
       'item_category4': "NA", 
       'price': productObj.baseOptions[0].selected.priceData.value, 
       'item_list_name': 'NA', 
       'item_list_id': 'NA',
       'index':productPosition 
     }]
  }
});

			}})
			     dataLayer.push({
			    	
			       'event': 'productClick',
			       'ecommerce': {
			         'click': {
			           'actionField': {'list': 'New arrivals'},      
			           'products': {
			             'name': productObj.name,                      
			             'id': productObj.code,
			             'price': productPrice,
			             'brand': productObj.brandCode,
			             'category': productObj.primaryCategoryCode,
						 'categoryName' : itemCategoryName,	     
