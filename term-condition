//function for finding current page type.
function trackPageType(){
	if(currentUrl.includes("/brands")){
		trackPageRelatedData(brandLandingPage,"PLP");
	}else if(currentUrl.includes("/brand/")){
		trackPageRelatedData(brandListingPage,"PLP");
	}else if(currentUrl.includes("personalshopper") && !currentUrl.includes("personalshoppers")){
		trackPageRelatedData("Personal Shopper Page","Personal Shopper");
		 _satellite.track('personal_shopper_page');
		 
	} else if(currentUrl.includes("mac-service")){
		trackPageRelatedData("Mac Service Page","Beauty Service");
	} 
	else if(""==null || ""==''){
		if(trackMacOrPersonalShopperPages())
			{
       //	page["pagecategory"]="Home";
			}
		else{
			page["pagecategory"]="Home".charAt(0).toUpperCase()+ "Home".slice(1).toLowerCase();
		}
			
	}else if("" == 'PRODUCT'){
		 trackPageRelatedData(pdp,"PDP");
	}
	else{
		page["pageName"]="".charAt(0).toUpperCase() + "".slice(1).toLowerCase();
        page["pagecategory"]="".charAt(0).toUpperCase() + "".slice(1).toLowerCase();	}
}
//function for tracking non checkout page data.
function trackNonCheckoutPageData(){
	var breadcrumbs = [
		                 "Home".toLowerCase(),
		   			  
		   				 "|Term and Condition".toLowerCase(),         
		   			  ]
		if (breadcrumbs.length == 1) {
			var pathName=window.location.pathname;
		    if (pathName !== null && pathName!=="/") {
		        breadcrumbs = [breadcrumbs + pathName.replace(/\//g, '|')]
		    }
		}
	var pageName = setBreadCrumbsData(breadcrumbs.length,breadcrumbs);
	if(pageName.includes("link first citizen card")){
		page["pageName"] = "FCC";
		page["pagecategory"] = "FCC"
	}else{
		page["pageName"]   =setBreadCrumbsData(breadcrumbs.length,breadcrumbs);	
	}
		page["channel"]    =setBreadCrumbsData(2,breadcrumbs);
		if(currentUrl.includes("/brand/")){
			page["subsection1"]=setSectionBreadCrumbsData(2,breadcrumbs);
		}else if(false){
			page["subsection1"]=setSectionBreadCrumbsData(2,breadcrumbs);
			page["subsection2"]=setSectionBreadCrumbsData(3,breadcrumbs);
			page["subsection3"]=setSectionBreadCrumbsData(4,breadcrumbs);		
		}else if(false)
		{
		page["subsection1"]=setBreadCrumbsData(3,breadcrumbs);
		page["subsection2"]=setBreadCrumbsData(4,breadcrumbs);
		page["subsection3"]=setBreadCrumbsData(5,breadcrumbs);
		}  
		checkBreadcrumbsAvailability();
		trackAuthentication();
		trackPageType();
		digitalData["page"] = page;	
}

//function for finding applied promotions on cart
function trackAppliedPromotions(){
    var productLevelPromotions = [
             	                  ]
    var orderLevelPromotions = [
                    	          ]
    var promotions=[];
    if(productLevelPromotions.length>0){
     promotions=productLevelPromotions
     }
    if(orderLevelPromotions.length>0){
        if(promotions.length>0){
           promotions=promotions+","+orderLevelPromotions
           }else{
            promotions=	orderLevelPromotions;
            }
      }
   digitalData["promocode"]=promotions;	
}


