function LoginTypeMobile(name)
{
	localStorage.removeItem('LoginorSignup');
	localStorage.setItem('Loginmethod', name);
	localStorage.setItem('LoginmethodTime', 1);
	localStorage.setItem('medium', "mobile_number");
	// GA_signup_initiate()
	// alert("bbc")
}
function signUp(){
	localStorage.setItem('LoginorSignup', "sign_up");
	localStorage.setItem('LoginmethodTime', 1);
	
}
function LoginTypeEmail(name)
{
	localStorage.removeItem('LoginorSignup');
	localStorage.setItem('Loginmethod', name);
	localStorage.setItem('LoginmethodTime', 1);
	localStorage.setItem('medium', "email");
	// GA_signup_initiate()

}
function GA_signup_initiate()
{
	
	
	// alert("cbc")
			var currentDevice = ""
		var phoneOrNot = !!navigator.maxTouchPoints;
		let phoneNum = "";
	
	var customer_type="NA"
	var user_type= "NA"
	var visitorType = "";

	if (sessionStorage.getItem("isLoggedIn") == "true") {
		visitorType = "loggedin"
		 user_type=localStorage.getItem("user_type_value") || "NA"
		 if(localStorage.getItem("user_type_value")){
	
	}
	} else {
		visitorType = "guest"
	}
	
		let prevpage="NA"
		var screenName="NA";
	
// alert(sessionStorage.getItem("prevPage"))
if(sessionStorage.getItem("prevPage")){
	let prevPageArray=JSON.parse(sessionStorage.getItem("prevPage"))
	if(prevPageArray.length>=1){
		screenName=prevPageArray[prevPageArray.length-1]
	}
	if(prevPageArray.length >=2){
		prevpage=prevPageArray[prevPageArray.length-2]
	}else if(prevPageArray.length ==1){
		prevpage="NA"
	}
}	
let userID="NA"
if (sessionStorage.getItem("isLoggedIn") == "true") {
	userID=localStorage.getItem("UserIdkey")
	
	} 
// $.ajax({
//                       url : "/my-account/isOrderPresent",
//                       type : "GET",
//                       async: false,
//                       success : function(res) {
				// if(res)
				// {
				// 	if (sessionStorage.getItem("isLoggedIn") == "true") {
				// 		customer_type="existing_customer"
				// 		}
				// 		else{
				// 			customer_type="NA"
				// 		}
					
				// }
				// else{
				// 		if (sessionStorage.getItem("isLoggedIn") == "true") {
				// 		customer_type="new_customer"
				// 		}
				// 		else{
				// 			customer_type="NA"
				// 		}

				// }
				var member_status="Non member"
				if(localStorage.getItem("GA_member_status")){
					if (sessionStorage.getItem("isLoggedIn") == "true") {
						member_status=localStorage.getItem("GA_member_status")
					}
				}
				// $.ajax({
                //       url : "/dy/customer/getUserDetails",
                //       type : "GET",
                //       async: false,
                //       success : function(data) {
					setTimeout(() => {
			const box = document.getElementsByClassName('regitration-header')[0];
			var medium = localStorage.getItem("medium");
			console.log(medium,"medium")
	
	if (box.style.display=="block") {
		dataLayer.push({'event':'signup_initiate',
						
						'userID':userID,
						'member_status':member_status , 
						'medium':medium,
						'screen_name':screenName,
						'previous_screen_name':prevpage
					});
	console.log('value is', box.style.display);
	} else {
	     dataLayer.push({'event':'signup_initiate',
						
						'userID':userID,
						'member_status':member_status , 
						'medium':medium,
						'screen_name':screenName,
						'previous_screen_name':prevpage
					});
	}
	}, 3000);	
						
						// alert("Ads")
				// 	  }
				// })

	//   }})
	
}
