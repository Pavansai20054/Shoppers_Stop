        a.push($( this ).text())
        })
                dataLayer.push({
                "event":"Category Viewed",
                "ecommerce":{
                        "currencyCode":"INR",
                        "category":{     
                        "category":url[3],
                        // "category":JSON.stringify(a[1]),
                        "brandName":"",
                        "brand_id":"",
                        "category2":"",
                        "category_id":idsss_new[1].slice(0,3),
                        "category2_id":"",
                        "path":window.location.href}
                }
        })
        }
        else if(url[3].split("?")[0]=="luxe")
        {
                
                dataLayer.push({
                "event":"Category Viewed",
                "ecommerce":{
                        "currencyCode":"INR",
                        "category":{
                
                        "category":"luxe",
                        "category":a[1] ? a[1] : "",
                        "category2":"",
                        "category_id":"",
                        "category2_id":"",
                        "brand_id":"",
                        "brandName":"",
                
