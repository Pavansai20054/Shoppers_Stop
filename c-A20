function GA_search_comp()
{
		localStorage.setItem("GA_searchWord",document.getElementById("js-site-search-input").value)	
}
 $(document).ready(function() {
	
	$(".js-site-search-input").keypress(function(){
	
		localStorage.setItem("getSearchKeyword",$(".search-container .search input").val())
				if (event.key === "Enter") {
					localStorage.setItem("GA_searchWord",document.getElementById("js-site-search-input").value)				
		}
});

