                }
        })
        }
        else if(url[3].split("?")[0]=="giftshop")
        {
                console.log("giftshop")
        dataLayer.push({
                "event":"Category Viewed",
                "ecommerce":{
                        "currencyCode":"INR",
                        "category":{
                
                        "category":"giftshop",
                
                        "category2":"",
                        "category_id":"GiftShopNavigationNode",
                        "category2_id":"",
