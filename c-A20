}

//to check the checkout registration initiate flag enabled or not	
	function verifyCheckoutRegistrationFlag()
	{
		if(localStorage.getItem("isCheckoutRegistration")=='true')
	    {
	    trackCheckoutRegistrationAction();
	    }
	}
	
// to check whether checkout signup is success or not
function trackCheckoutRegistrationAction()
	{
		if("" == 'failed')
			{
			trackRegistrationFailure();
			}
		else if("false"=='true')
			{
			trackRegistrationSuccess();
			}
		localStorage.setItem("isCheckoutRegistration", false);
		} 

//to track login or registration action and login type details (.i.e common method for all email, google account and facebook tracking details)
function trackLoginOrRegistration(action, loginType)
  {
		 Login["action"]=action;
		 Login["logintype"]=loginType;
		 digitalData["Login"]=Login;
		_satellite.track('login'); 
	}

//on click of personal shoppers banner
$(document).find('.responsive-padzero').on('click','.main-slider ul',function(e) {
	if($(this).find('a').attr('href').indexOf('personalshopper') > -1){
	_satellite.track('PS_landing_click');
	}
});

//on click of personal shoppers BOOK NOW button
$(document).on('click', '.new-world-shopping .content img', function(e) {
	_satellite.track('PS_book_now_click');
});

//on click of personal shoppers continue button in store finder page
$(document).on('click', '.store_leftside #storeFinderForm #storecontinueBtnId', function(e) {
	var macBookingId = $('#macBeautyId').val() != null  ? $('#macBeautyId').val() : '798330002'; 
	if($('#bookingTypeId').val() == macBookingId){
		_satellite.track('mac_store_select_continue');
	}
	else{
		_satellite.track('ps_store_select_continue');
	}

});

//on click of personal shoppers continue button in store finder map
$(document).on('click', '#mapBookBtn', function(e) {
	
	var macBookingId = $('#macBeautyId').val() != null  ? $('#macBeautyId').val() : '798330002'; 
	if($('#bookingTypeId').val() == macBookingId){
		_satellite.track('mac_store_select_continue');
	}
	else{
		_satellite.track('ps_store_select_continue');
	}
	
});

//on click of personal shoppers continue button in service page
$(document).on('click', '#servicesbtn', function(e) {
	_satellite.track('PS_appointment_click');
});

//on click of personal shoppers book now button in personal shoppers register page
$(document).on('click', '#booknowbtn', function(e) {
	if($('#createAppointmentForm').valid()){
		var macBookingId = $('#macBeautyId').val() != null  ? $('#macBeautyId').val() : '798330002'; 
		if($('#bookingTypeId2').val() == macBookingId){
			_satellite.track('mac_customer_page_submit');
		}
		else{
			 _satellite.track('ps_customer_page_submit');
		}
	}
}); 

/**
 * to track the store selction /customer information page 
 */
function trackMacOrPersonalShopperPages(){
	if(typeof "" != 'undefined' && "" != null && "" != '' && "" != 'undefined' && "" != null && "" != '' ){
		if("" == '798330001'){
			page["pageName"]='PS Store Selection Page';
			trackPSCategory();
			_satellite.track('ps_store_selection_page');
			return true;
		}else if("" == '798330002'){
			page["pageName"]='Mac Store Selection Page';
			trackMacServiceCategory();
			_satellite.track('mac_store_selection_page');
			return true;
		}
		return false;
}else if(typeof "" != 'undefined' && ""  != null && "" != '' && "" != 'undefined' && "" != null && "" != '' ){
		if("" == '798330001'){
			page["pageName"]='PS Customer Information Page';
			trackPSCategory();
			_satellite.track('ps_customer_information_page');
			return true;
		}else if("" == '798330002'){
			page["pageName"]='Mac Customer Information Page';
			trackMacServiceCategory();
			_satellite.track('mac_customer_information_page');
			return true;
		}
		return false;
}
	return false;
}

//personal shopper category tracking
function trackPSCategory(){
	page["pagecategory"] = "Personal Shopper";
}

//mac  category tracking
function trackMacServiceCategory(){
	page["pagecategory"] = "Beauty Service";
}

//to track the payment modes
function trackPaymentMode(paymentmode){
	if(paymentmode == 'COD Payment'){
		return "COD";
	}else if(paymentmode == 'credit card'){
		return 'CREDIT CARD';
	}else if(paymentmode == 'debit card'){
		return 'DEBIT CARD';
	}else if(paymentmode == 'debit card'){
		return 'DEBIT CARD';
	}else if(paymentmode == 'ss credits payment'){
		return 'SS WALLET';	
	}else if(paymentmode == 'Paytm Payment Gateway'){
		return 'PAYTM';	
	}else if(paymentmode == 'gift caed / e-gift voucher'){
		return 'GIFT CARD / E-GIFT VOUCHER';	
	}else if(paymentmode == 'CASH ON DELIVERY'){
		return "COD";
	}else{
		return paymentmode;
	}
 }


