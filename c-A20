						member_status=localStorage.getItem("GA_member_status")
					}
				}
	if(localStorage.getItem("GA_searchWord"))
	{
			 dataLayer.push({'event':'search_completed',   
        'userID':userID,
        'member_status':member_status,
        'search_term':localStorage.getItem("GA_searchWord"),
        'search_method':"type",
        'screen_name':screenName,
        'previous_screen_name':prevpage
		});
		localStorage.removeItem("GA_searchWord")
	}
  




