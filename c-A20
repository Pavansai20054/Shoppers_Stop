                }
        })
        }
        else if(url[3].split("?")[0]=="promotions")
        {
                dataLayer.push({
                "event":"Category Viewed",
                "ecommerce":{
                        "currencyCode":"INR",
                        "category":{
                
                        "category":BargainsorLuxe1,
                        
                        "category2":"",
                        "category_id":"",
