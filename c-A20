//function for finding categories data.
var firstTime = 0;
function trackCategoriesData(entry){
	var category=entry;
	var str=category.replace("[","");
	str=str.replace("]","");	
	var splits=str.split(", ");
	$.each(splits, function (index, value) {
		if(index!=0){
		if(firstTime == 0){
			page["subsection"+index] = [];
		}
		}
		});
	$.each(splits, function (index, value) {
		if(index!=0){
		page["subsection"+index].push($.trim(value));
		firstTime = 1;
		}
		});
}
//function for finding multiple categories data in checkout flow.
function trackMultipleCategoriesData(){
	
	    page["subsection1"]=[];
	    page["subsection2"]=[];
	    page["subsection3"]=[];
	    
	}
//function for finding multiple products categories data in order confirmation page.
function trackOrderCategoriesData(){
	
}

