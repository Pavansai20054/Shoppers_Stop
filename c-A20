//Call this function when user clicks on facet filters
$(document).on('click','.filter-inner .ex-checkbox label',function(e){
	var facet = $(this).attr('for').trim();
    var facetCategory=$(this).closest(".filter-wpr").find(".filter-category-heading").text().trim().split('\t')[0].trim()
	if($(this).siblings('.facet-checkbox').prop('checked'))
    {
      return;		
    }	
    else{
    	digitalData["filters"] = facetCategory+":"+facet;
    	digitalData["action"] = "secondaryfilter";
    	_satellite.track('filter');
    }
 
});
//call this function when user cliks on top filters in plp
$('#product-facet').on('click', '.tab_content .tabs_item ul li', function(){
	var filterMainCategory=$('#product-facet').find('.bx-wrapper').find('.tabs').find('.current').find('span').text().trim();
	var filterSubCategory=$(this).text().trim().split('\t')[0].trim();
	digitalData["filters"] = filterMainCategory+":"+filterSubCategory;
	digitalData["action"] = "secondaryfilter";
	    	_satellite.track('filter');
	});

