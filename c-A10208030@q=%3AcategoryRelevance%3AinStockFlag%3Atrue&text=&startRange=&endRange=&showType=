   }

//Measure the user wish list 
  function trackAddToWishlist(productCode){
	
	console.log("bbc----------")
	// alert("saddas")
	$.ajax({
			type : "GET",
			url : window.location.origin+"/dy/customer/getWishListData",
			success : function(response) {
				console.log(response);
            	console.log("wishlist data");
				let priceOfProds=0;
				for(let i=0;i<response.length;i++)
				{
						priceOfProds=priceOfProds+response[i].price.value
				}
				console.log(priceOfProds)
				let prodDetails=[]
				for(let i=0;i<response.length;i++)
				{
					let wasPrice=0
					let curPrice=0
					if(response[i].price)
					{
						wasPrice=response[i].price.value
					}else if(response[i].price){
						wasPrice=response[i].price.value
					}
					if(response[i].price)
					{
						curPrice=response[i].price.value
					}
					else if(response[i].originalPrice)
					{
						curPrice=response[i].originalPrice.value
					}
					prodDetails.push({
						brand:response[i].brandName,
						brand_id:response[i].brandCode,
						price:parseFloat(wasPrice.toFixed(2)),
						image:[response[i].images ? response[i].images[3].url :""],
						name:response[i].name,
						id:response[i].code,
						discount:parseFloat((wasPrice-curPrice).toFixed(2)),
						retail_price:parseFloat(curPrice.toFixed(2))

					})	
				}
			if(sessionStorage.getItem("isLoggedIn") =="true")
			{		
				dataLayer.push({
								"event":"wishlistUpdated",
								"ecommerce":{
								"currencyCode":"INR",
								"wishlist-updated":{
								"no_of_items":response.length,
								"total_amount":priceOfProds,
								"products":prodDetails
								}
							}
							})
			}
			},
			error : function(xhr, status, error) {
				
				console.log(error)
			
			
			}
		});
	   digitalData["pid"] = productCode;
	   digitalData["action"] ="addtowishlist";
	  _satellite.track('checkout');
	  _satellite.track('cpj_add_to_wishlist'); 
  }
  function trackRemoveFromWishlist(productCode){
	console.log("bbc----------abc");
	// alert("asddsa")
	$.ajax({
			type : "GET",
			url : window.location.origin+"/dy/customer/getWishListData",
			success : function(response) {
				console.log(response);
            	console.log("wishlist data");
				let priceOfProds=0;
				for(let i=0;i<response.length;i++)
				{
						priceOfProds=priceOfProds+response[i].price.value
				}
				console.log(priceOfProds)
				let prodDetails=[]
				for(let i=0;i<response.length;i++)
				{
					let wasPrice=0
					let curPrice=0
					if(response[i].price)
					{
						wasPrice=response[i].price.value
					}else if(response[i].price){
						wasPrice=response[i].price.value
					}
					if(response[i].price)
					{
						curPrice=response[i].price.value
					}
					else if(response[i].originalPrice)
					{
						curPrice=response[i].originalPrice.value
					}
					prodDetails.push({
						brand:response[i].brandName,
						brand_id:response[i].brandCode,
						price:parseFloat(wasPrice.toFixed(2)),
						image:[response[i].images ? response[i].images[3].url :""],
						name:response[i].name,
						id:response[i].code,
						discount:parseFloat((wasPrice-curPrice).toFixed(2)),
						retail_price:parseFloat(curPrice.toFixed(2))

					})	
				}
			if(sessionStorage.getItem("isLoggedIn")=="true")
			{		
				dataLayer.push({
								"event":"wishlistUpdated",
								"ecommerce":{
								"currencyCode":"INR",
								"wishlist-updated":{
								"no_of_items":response.length,
								"total_amount":priceOfProds,
								"products":prodDetails
								}
							}
							})
			}
			},
			error : function(xhr, status, error) {
				
				console.log(error)
			
			
			}
		});
	  digitalData["pid"] = productCode;
	  digitalData["action"] ="removefromwishlist";
	  _satellite.track('checkout');
	  _satellite.track('cpj_remove_from_wishList');
  }
  

