        }
        else if("HOME"=="CATEGORYLANDING")
        {
                var a=[]
                $(".breadcrumb").find("li").each(function( index ) {
        a.push($( this ).text())
        })
                dataLayer.push({
                "event":"Category Viewed",
                "ecommerce":{
                        "currencyCode":"INR",
                        "category":{     
                        "category":url[3],
                        // "category":JSON.stringify(a[1]),
                        "brandName":"",
