/**
 * Call this function when a user clicks on a product link. This function uses the event
 * callback datalayer variable to handle navigation after the ecommerce data has been sent
 * to Google Analytics.
 * @param {Object} productObj An object representing a product.
 */
//Product click on new arrival on HomePage
 var clickedProduct,productObj,getinputval;

$('.new-arrivals ul li a').on('click',function(e) {
	
   getinputval = $(this).find('input').val();
   //this piece of code is added to handle the exception in case href does not conatain (p-,?)
   var productCode;
   var href=$(this).attr('href');
   try{
   if(href.includes("/p-")){
 	 href = href.split("/p-")[1];
   if(href.includes('?')){
     productCode = href.split('?')[1];
     }
   }
   }
   catch(err)
   {
 console.log(err);
   }
   var clickedProductDetailsUrl = "/cart/productBasicDetails";
   		
	   $.ajax({
		    data : {
		     'productCode' : productCode
		    },
		    type : 'Get',
		    url : clickedProductDetailsUrl,
		    success : HomeObject,
		    error : function(jqXHR, textStatus, errorThrown) {
		     console.log(JSON.stringify(jqXHR));
		     console.log("AJAX error: " + textStatus + ' : ' + errorThrown);
		    }
	   });
	   
	   
			   function HomeObject(productObj) {
				console.log("HomeObject")
			
				dataLayer.push({ ecommerce: null }); 
				var currentDevice = ""
		var phoneOrNot = !!navigator.maxTouchPoints;
	let phoneNum = "";

	if (phoneOrNot) {
		if (navigator.userAgent.match(/Android/i)) {
			currentDevice = "android"
		} else {
			currentDevice = "ios"
		}
	} else {
		currentDevice = "desktop"
	}

	dataLayer.push({
		ecommerce: null
	});
	var visitorType = "";
	if (sessionStorage.getItem("isLoggedIn") == "true") {
		visitorType = "loggedin"
	} else {
		visitorType = "guest"
	}
	if (sessionStorage.getItem("isLoggedIn") == "true") {
		phoneNum = "+91" + localStorage.getItem("userNumber")
	} else {
		phoneNum = "NA"

	}
		let prevpage="NA"
// alert(sessionStorage.getItem("prevPage"))
if(sessionStorage.getItem("prevPage")){
	let prevPageArray=JSON.parse(sessionStorage.getItem("prevPage"))
	if(prevPageArray.length >=2){
		prevpage=prevPageArray[prevPageArray.length-2]
	}else if(prevPageArray.length ==1){
		prevpage="NA"
	}
}	
let userID="NA"
if (sessionStorage.getItem("isLoggedIn") == "true") {
	userID=localStorage.getItem("UserIdkey")
	
	} else {
