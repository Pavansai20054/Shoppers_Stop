//Call this function when a user clicks on signup button.
$(document).on('click', '.login-with-social .signUp-btn,.mob a[href="javascript:void(0);"]', function(e) {
	if (""=="CHECKOUT" && requestUri.search("login") >= 0) {
    trackPageCategoryData(checkoutSignup,checkout);
    firstTime=0;
    trackMultipleCategoriesData();
    _satellite.track('checkout-link');
	}
});

//Call this function when a user clicks on Guest signin  link.
$(document).on('click', '.continue-guest,a[href="#guest-user"]', function(e) {
