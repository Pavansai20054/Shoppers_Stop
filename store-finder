	function getDecodedCookie (cookie){
		return decodeCookie(getCookie(cookie),10);
	};
	function decodeCookie(cookieValue,count){
		if(cookieValue !== undefined && count > 0){
			try {
				value = window.atob(cookieValue);
			}catch(err){
				value = decodeCookie(cookieValue+"=", count-1);
			}
		}else{
			value ="";
		}
		return value;
	};
	function getUserData(){
		var cookieValue = getDecodedCookie("uniqueUserId");
		var userData;
		try{
			userData = JSON.parse(cookieValue);
		}catch(err){
			//By default set user to Anonymous
			userData = JSON.parse('{"userId":"anonymous"}');
		}
		return userData;
	};
	function getCookie(cname) {
	    var name = cname + "=";
	    var ca = document.cookie.split(';');
	    for(var i = 0; i < ca.length; i++) {
	        var c = ca[i];
	        while (c.charAt(0) == ' ') {
	            c = c.substring(1);
	        }
	        if (c.indexOf(name) == 0) {
	            return c.substring(name.length, c.length);
	        }
	    }
	    return "";
	};
