	 var id=$(".fav-banner-button").attr("data-select-uid");
	 var name=$(".fav-banner-button").attr("data-select-name");
	 var position=$(".fav-banner-button").attr("data-select-position");
	 var creative=$(".fav-banner-button").attr("data-select-creative");
	 var promoObj = {id:id, name:name, pos:position, creative:creative, destinationUrl:link};

	 onPromoClick(promoObj);
});

