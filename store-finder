$(document).ready(function() {
var url = window.location.href.split("/");
let brandName=url[3].split("?")[0].split("-")[0].toUpperCase()
let FullBrandName=""
let FirstName=""
let SecondName=""
try{
FullBrandName=url[3].split("?")[0].split("-")[0].toUpperCase()+" "+url[3].split("?")[0].split("-")[1].toUpperCase()
FirstName=url[3].split("?")[0].split("-")[0].toUpperCase()
SecondName=url[3].split("?")[0].split("-")[1].toUpperCase()
}
catch{
FullBrandName=url[3].split("?")[0].split("-")[0].toUpperCase()
FirstName=url[3].split("?")[0].split("-")[0].toUpperCase()
SecondName=""
}
// if(url[3].split("?")[0].split("-")[1])
// {
//      FullBrandName=FullBrandName+" "+url[3].split("?")[0].split("-")[1].toUpperCase()
// }
$.ajax({
			type : "GET",
			url : window.location.origin+"/dy/customer/getBrandId?brandNameSuffix="+FirstName+"&brandNamePreffix="+SecondName,
			success : function(response) {
				console.log(response);
            	                console.log("wishlistttt data")
                                if(response  && ""!="PRODUCT"  &&  ""=="" && ""!="CART"  && ""!="CATEGORY" && url[3]!="brands")
                                {
                                        dataLayer.push({"event":"brand_viewed","ecommerce":{
                                                "currencyCode":"INR",
                                                "brand":{
                                                "name":FullBrandName,
                                                "id":response}}});
                                }
                             
			},
			error : function(xhr, status, error) {
				// alert('');
			console.log("not happened")
			}
			});

})


