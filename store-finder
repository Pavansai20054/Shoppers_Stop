//Call this function when user clicks on checkout login
function trackCheckoutLogin(){
	 if("" == 'CHECKOUT'){
		if($('#j_username').val()!==null && $('#j_username').val()!=='' && $('#j_password').val()!==null && $('#j_password').val()!==''){
      var link={};
      link["linkName"]="home|login to delivery";
      digitalData["link"]=link;
      _satellite.track('int-link');
  	var username=$('#j_username').val()
	var loginMode=trackingModeForNormalLogin(username);
  	trackLoginOrRegistration("logininitiate", loginMode);	
	  }
	 }
}

