$(document).ready(function() {
       
        var url_one = window.location.href.split("/");
var itemcounttt=$(".plpH2TagCategoryNameDiv .heading").text().trim();
var counteee = itemcounttt;
var countvar=counteee.split(" ");
console.log(countvar);
console.log(counteee);
console.log("searchkey677667")



if(url_one[3]=="search")
{
      
        console.log("inside search")
       
	dataLayer.push({"event":"search","search_term":url_one[4].split("?")[1].split("=")[1].replaceAll("%20"," "),"item_count":parseInt(countvar[0])=="NaN" ? 0 : parseInt(countvar[0])});
        dataLayer.push({
	 'event': 'productImpression',
	  'ecommerce': {
	    'currencyCode': 'INR',
	    'impressions': [	
				{
					'name': "", 					
					'price': "",
					'brand': "",
					'brand_id': "",
					'variant': "",
					'list': 'Search Results',
					'category2':"",
					'category3':"",
					'category4':"",
					'category': "",	
					'category_id':"",				
					'category4_id':"",
					'category2_id':"",
					'category3_id':'',
					'path':window.location.href,		
					'search_term':url_one[4].split("?")[1].split("=")[1].replaceAll("%20"," ")		
				}		
	                  ]
	  }
	});
}
else if( url_one[4]=="search")
{

          console.log("inside search")
          if(countvar[0])
          {
                dataLayer.push({"event":"search","search_term":url_one[4].split("?")[1].split("=")[1],"item_count":parseInt(countvar[0])=="NaN" ? 0 : parseInt(countvar[0])});
          }
          else{
                 dataLayer.push({"event":"search","search_term":localStorage.getItem("getSearchKeyword"),"item_count": 0 });
          }
	
}
else if(""=="PRODUCTSEARCH")
{
          console.log("inside search PRODUCTSEARCH")
          if(countvar[0] && url_one[3]!="brand" )
          {
                dataLayer.push({"event":"search","search_term":url_one[4].split("?")[1].split("=")[1],"item_count":parseInt(countvar[0])=="NaN" ? 0 : parseInt(countvar[0])});
          }
          else if(url_one[3]!="brand"){
                 dataLayer.push({
	 'event': 'productImpression',
	  'ecommerce': {
	    'currencyCode': 'INR',
	    'impressions': [	
				{
					'name': "", 					
					'price': "",
					'brand': "",
					'brand_id': "",
					'variant': "",
					'list': 'Search Results',
					'category2':"",
					'category3':"",
					'category4':"",
					'category': "",	
					'category_id':"",				
					'category4_id':"",
					'category2_id':"",
					'category3_id':'',
					'path':window.location.href,		
					'search_term':url_one[4].split("?")[1].split("=")[1].replaceAll("%20"," ")		
				}		
	                  ]
	  }
	});
                 dataLayer.push({"event":"search","search_term":url_one[3].split("?")[1].split("=")[1].replaceAll("%20"," "),"item_count": 0 });
          }
	
}
})



//for category view event
$(document).ready(function() {
var url = window.location.href.split("/");
let brandName=url[3].split("?")[0].split("-")[0].toUpperCase()
let FullBrandName=""
let FirstName=""
let SecondName=""
try{
FullBrandName=url[3].split("?")[0].split("-")[0].toUpperCase()+" "+url[3].split("?")[0].split("-")[1].toUpperCase()
FirstName=url[3].split("?")[0].split("-")[0].toUpperCase()
SecondName=url[3].split("?")[0].split("-")[1].toUpperCase()
}
catch{
FullBrandName=url[3].split("?")[0].split("-")[0].toUpperCase()
FirstName=url[3].split("?")[0].split("-")[0].toUpperCase()
SecondName=""
}
// if(url[3].split("?")[0].split("-")[1])
// {
//      FullBrandName=FullBrandName+" "+url[3].split("?")[0].split("-")[1].toUpperCase()
// }
$.ajax({
			type : "GET",
			url : window.location.origin+"/dy/customer/getBrandId?brandNameSuffix="+FirstName+"&brandNamePreffix="+SecondName,
			success : function(response) {
				console.log(response);
            	                console.log("wishlistttt data")
                                if(response  && ""!="PRODUCT"  &&  ""=="" && ""!="CART"  && ""!="CATEGORY" && url[3]!="brands")
                                {
                                        dataLayer.push({"event":"brand_viewed","ecommerce":{
                                                "currencyCode":"INR",
                                                "brand":{
                                                "name":FullBrandName,
                                                "id":response}}});
                                }
                             
			},
			error : function(xhr, status, error) {
				// alert('');
			console.log("not happened")
			}
			});

})


