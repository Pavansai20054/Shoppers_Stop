	_satellite.track('PS_appointment_click');
});

//on click of personal shoppers book now button in personal shoppers register page
$(document).on('click', '#booknowbtn', function(e) {
	if($('#createAppointmentForm').valid()){
		var macBookingId = $('#macBeautyId').val() != null  ? $('#macBeautyId').val() : '798330002'; 
		if($('#bookingTypeId2').val() == macBookingId){
			_satellite.track('mac_customer_page_submit');
		}
		else{
			 _satellite.track('ps_customer_page_submit');
		}
	}
}); 

/**
 * to track the store selction /customer information page 
 */
function trackMacOrPersonalShopperPages(){
	if(typeof "" != 'undefined' && "" != null && "" != '' && "" != 'undefined' && "" != null && "" != '' ){
		if("" == '798330001'){
			page["pageName"]='PS Store Selection Page';
			trackPSCategory();
			_satellite.track('ps_store_selection_page');
			return true;
		}else if("" == '798330002'){
			page["pageName"]='Mac Store Selection Page';
			trackMacServiceCategory();
			_satellite.track('mac_store_selection_page');
			return true;
		}
		return false;
}else if(typeof "" != 'undefined' && ""  != null && "" != '' && "" != 'undefined' && "" != null && "" != '' ){
		if("" == '798330001'){
			page["pageName"]='PS Customer Information Page';
			trackPSCategory();
			_satellite.track('ps_customer_information_page');
			return true;
		}else if("" == '798330002'){
			page["pageName"]='Mac Customer Information Page';
			trackMacServiceCategory();
			_satellite.track('mac_customer_information_page');
			return true;
		}
		return false;
}
	return false;
}

//personal shopper category tracking
function trackPSCategory(){
	page["pagecategory"] = "Personal Shopper";
}

//mac  category tracking
function trackMacServiceCategory(){
	page["pagecategory"] = "Beauty Service";
}

//to track the payment modes
function trackPaymentMode(paymentmode){
	if(paymentmode == 'COD Payment'){
		return "COD";
	}else if(paymentmode == 'credit card'){
		return 'CREDIT CARD';
	}else if(paymentmode == 'debit card'){
		return 'DEBIT CARD';
	}else if(paymentmode == 'debit card'){
		return 'DEBIT CARD';
	}else if(paymentmode == 'ss credits payment'){
		return 'SS WALLET';	
	}else if(paymentmode == 'Paytm Payment Gateway'){
		return 'PAYTM';	
	}else if(paymentmode == 'gift caed / e-gift voucher'){
		return 'GIFT CARD / E-GIFT VOUCHER';	
	}else if(paymentmode == 'CASH ON DELIVERY'){
		return "COD";
	}else{
		return paymentmode;
	}
 }


</script> 	<script type="text/javascript">

var pageType=document.getElementById("pageType").value;

function trackDYLogin()
{
    localStorage.setItem('loginEvent', 'true')
}

function trackDYRegistration(email,gender){
  if (typeof DY.API === "function") {
  
     if(typeof email !== "undefined" && email !== ""){
         DY.API("event", {
                 name: "Signup",
                 properties: {
                     dyType: "signup-v1",
                     hashedEmail: DYO.dyhash.sha256(email),
                     metadata: {
                     gender : gender
                 }
             }
         });
         trackDYIdentifyAPI(email);
     }
  }
}


function trackDYPromoCodeEntered(){
 
 if (typeof DY.API === "function") {


     var orderLevelPromotions = [
                    	               ];
    $.each(orderLevelPromotions, function(){
    	if(typeof this.valueOf() !== "undefined"){
            DY.API("event", {
                name: "Promo Code Entered",
                properties: {
                    dyType: "enter-promo-code-v1",
                    code: this.valueOf()
                    }
                });
        }
    });
 }
 
}

function trackDYNewsletterSubscription(email){
    if (typeof DY.API === "function") {
        DY.API("event", {
            name: "Newsletter Subscription",
            properties: {
                dyType: "newsletter-subscription-v1",
                hashedEmail: DYO.dyhash.sha256(email)
            }
        });
        trackDYIdentifyAPI(email);
    }
}


if(pageType=='PRODUCTSEARCH' || pageType=='CATEGORY' || pageType == 'CATEGORYBRAND' ){
    if (typeof DY.API === "function") {
        var sortItem = $("#sort-by-list").find(".active").attr("sortCode"), sortBy, sortOrder;
        var map = new Object();
        map["categoryRelevance"] = "RELEVANCE,ASC";
        map["topRated"] = "POPULARITY,ASC";
        map["newArrival-desc"] = "NEW,DESC";
        map["discount-desc"] = "DISCOUNT,DESC";
        map["bestSeller-desc"] = "BESTSELLER,DESC";
        map["price-asc"] = "PRICE,ASC";
        map["price-desc"] = "PRICE,DESC";
        if (typeof sortItem !== 'undefined') {
            if (typeof map[sortItem] == 'undefined' || typeof map[sortItem] == '') {
                sortOrder = "ASC";
                sortBy: "OTHER";
            } else {
                sortBy = map[sortItem].split(',')[0];
                sortOrder = map[sortItem].split(',')[1];
            }
            DY.API("event", {
                name: "Sort Items",
                properties: {
                    dyType: "sort-items-v1",
                    sortBy: sortBy,
                    sortOrder: sortOrder,
                }
            });
        }
    }
}

if(pageType=='PRODUCTSEARCH'){
    if (typeof DY.API === "function") {
        if (typeof search.term != 'undefined') {
            DY.API("event", {
                name: "Keyword Search",
                properties: {
                    dyType: "keyword-search-v1",
                    keywords: search.term
                }
            });
        }
    }
}


$(document).on('click','.filter-inner .ex-checkbox label',function(e){
    if (typeof DY.API === "function") {
        var facet = $(this).attr('for');
        var facetCategory = $(this).closest(".filter-wpr").find(".filter-category-heading").text().trim().split('\t')[0].trim()
        if ($(this).siblings('.facet-checkbox').prop('checked')) {
            return;
        } else {
            DY.API("event", {
                name: "Filter Items",
                properties: {
                    dyType: "filter-items-v1",
                    filterType: facetCategory,
                    filterStringValue: facet
                }
            });
        }
    }
});

if(pageType=='ORDERCONFIRMATION'){
    if (typeof DY.API === "function") {
        var orderId = 'null';
	if(orderId != ''){
    	trackDYPromoCodeEntered();
        if($('[data-id="isFccProductPurchase"]').val()=="true" || $('[data-id="isFccProductPurchase"]').val()==true){
        	trackDYFirstCitizenPurchase();
        }
        DY.API("event", {
            name: "Purchase",
            properties: {
                uniqueTransactionId: "",
                
                dyType: "purchase-v1",
                value: parseFloat(),
                currency: "INR",
                cart: [
                    
                ]
            }
        });
	}
    }
}

if(pageType=='CART'){

    if (typeof DY.API === "function") {

        var cartAttributes = {
          
        };
    if(cartAttributes.cartOperation== "REMOVE" && typeof cartAttributes.code !== "undefined" && cartAttributes.code !== ""){
        DY.API("event", {
            name: "Remove from Cart",
            properties: {
                dyType: "remove-from-cart-v1",
                value: parseFloat(cartAttributes.price),
                currency: "INR",
                productId: cartAttributes.code,
                quantity: parseInt(cartAttributes.quantity),
                size: cartAttributes.size,
                cart: [
                    
                ]
            }
        });
    }
    else if(cartAttributes.cartOperation == "UPDATE"){
        DY.API("event", {
            name: "Sync cart",
            properties: {
                dyType: "sync-cart-v1",
                currency: "INR",
                cart: [
                    
                ]
            }
        });
    }
    }
}

function trackDYAddtoWishlist(productId){

    if (typeof DY.API === "function") {
        DY.API("event", {
            name: "Add to Wishlist",
            properties: {
                dyType: "add-to-wishlist-v1",
                productId: productId,
            }
        });
    }
}
