//calls this function when user clicks on submit button in retun/exchange popup.
 $(document).on('click', '.return-submit #returnSubmit', function(e) {
  var returnModalTabActive = $('#return-exchange-items_v1').find('#retExchangeRadio .acc-radio .optionsRadios:checked').next().find('span').text();		
  if(returnModalTabActive == "Return"){
		_satellite.track('Returns_initiated')
		returnReason = $('#return-exchange-items_v1').find('#custom-form-id-return').val();
		returnMode = $('#return-exchange-items_v1').find('#orderDetailPickUp .pickupOption .acc-radio .optionsRadios:checked').next().find('span').text(); 
		returnRefundMode = $('#return-exchange-items_v1').find('#orderDetailRefund .refundOption .acc-radio .optionsRadios:checked').next().find('span').text().replace(/\s+/g, ' ');
		productbrandname = $("#productBrandCodeName").val();
		returnOrExchangeProductCategoryNames=$("#returnProductCategoryNames").val();
		$('#return-exchange-items_v1').find('.variant_size_ulClass').each(function(index){
			returnQuantity=(index+1);
		});
		if(returnReason!="" && returnMode!="" && returnRefundMode!=""){
		returns["reason"]=returnReason;
		returns["mode"]=returnMode;
		returns["brand"]=productbrandname;
		returns["returnQuantity"]=returnQuantity;
		
		if(returnRefundMode == "Shoppers Stop Wallet"){
			returns["returnRefundMode"]="Wallet";	
		}else{
			returns["returnRefundMode"]="Online";
		}
		
		returns["size_sku"]= $('#return-modal-class').find('.returnProdCode').val();
		trackProductCategories(returnOrExchangeProductCategoryNames,returns);
		product["return"]=returns;
		digitalData["product"]=product;
		}
	}
	else if(returnModalTabActive == "Exchange"){
		_satellite.track('Exchange_initiated');
		exchangeReason = $('#return-exchange-items_v1').find('#custom-form-id-exchange').val();
		$('#return-exchange-items_v1').find('.variant_size_ulClass').each(function(index){
			exchangeSize_sku[index] = $(this).find('li.selected .variantSizeCode').val();
			exchangeSize[index] = $(this).find('li.selected button').text();
			exchangeQuantity=(index+1);
		})
		exchangeMode = $('#return-exchange-items_v1').find('#orderDetailPickUp .pickupOption .acc-radio .optionsRadios:checked').next().find('span').text();
		productbrandname = $("#productBrandCodeName").val();
		returnOrExchangeProductCategoryNames=$("#returnProductCategoryNames").val();
		if(exchangeReason!="" && exchangeMode!=""){
		exchange["reason"]=exchangeReason;
		exchange["quantity"]=exchangeQuantity;
		exchange["size_sku"]=exchangeSize_sku;
		exchange["size"]=exchangeSize;
		exchange["brand"]=productbrandname;
		trackProductCategories(returnOrExchangeProductCategoryNames,exchange);
		product["exchange"]=exchange;
		digitalData["product"]=product;
		}
	}	
 });
//function for finding categories data in return/exchange.
 var firstTimeReturn = 0;
 function trackProductCategories(entry,parent){
 	var category=entry;
 	var str=category.replace("[","");
 	str=str.replace("]","");	
 	var splits=str.split(", ");
 	$.each(splits, function (index, value) {
 		if(index!=0){
 		if(firstTime == 0){
 			parent["subsection"+index] = [];
 		}
 		}
 		});
 	$.each(splits, function (index, value) {
 		if(index!=0){
 		parent["subsection"+index].push($.trim(value));
 		firstTimeReturn = 1;
 		}
 		});
 }
 //call this function when user successfully logged in through email or mobile number
 function trackLoginSuccess(loginMode)
 {
	 trackLoginOrRegistration("loginsuccess", loginMode);
 }
 
 //call this function when user login fails through email or mobile number
 function trackLoginFailure(loginMode)
 {
	 trackLoginOrRegistration("loginfailure", loginMode);
 }
 
 //call this function when user registers successfully through email
 function trackRegistrationSuccess(){
	 trackLoginOrRegistration("registrationsuccess", "email");
 }
 
 //call this function when user registration fails
 function trackRegistrationFailure(){
	 trackLoginOrRegistration("registrationfailure", "email");
 }
 
//call this funtion whenever sorting is applied
function trackSortOption(){
	digitalData["sortoption"]=$("#sort-by-list").find(".active").text().trim();
    digitalData["action"]="sort";
    _satellite.track('sort_option_tracking');
}

//call this function when user successfully logged in through facebook
function trackFacebookLoginSuccessResponse()
{
	trackLoginOrRegistration("loginsuccess", "facebook");
}

//call this function when user login failed through facebook
function trackFacebookLoginFailureResponse()
{
	trackLoginOrRegistration("loginfailure", "facebook");
}

//call this function when user successfully logged in  through google account
function trackGoogleLoginSuccessResponse()
{
	trackLoginOrRegistration("loginsuccess", "google");
}

//call this function when user login fails  through google account
function trackGoogleLoginFailureResponse()
{
	trackLoginOrRegistration("loginfailure", "google");
}

//to check the checkout registration initiate flag enabled or not	
	function verifyCheckoutRegistrationFlag()
	{
		if(localStorage.getItem("isCheckoutRegistration")=='true')
	    {
	    trackCheckoutRegistrationAction();
	    }
	}
	
// to check whether checkout signup is success or not
function trackCheckoutRegistrationAction()
	{
		if("" == 'failed')
			{
			trackRegistrationFailure();
			}
		else if("false"=='true')
			{
			trackRegistrationSuccess();
			}
		localStorage.setItem("isCheckoutRegistration", false);
		} 

//to track login or registration action and login type details (.i.e common method for all email, google account and facebook tracking details)
function trackLoginOrRegistration(action, loginType)
  {
		 Login["action"]=action;
		 Login["logintype"]=loginType;
		 digitalData["Login"]=Login;
		_satellite.track('login'); 
	}

