//calls this function when user clicks on size button in PDP page.
$(document).on('click', '.variant_size_ulClass li button', function(e) {
	if(false){
    var size=$(this).closest('li').find('.variantSizeCode').val();
    trackNonCheckoutPageData();
    trackPageRelatedData(pdp,"PDP");
    product[masterProductId]=[""];
    product[colorVariantId]=[""];
    product[brandId]=[""];
    product[sizeVariantId] =[size];
    digitalData["product"] =product;
    digitalData["productfindingmethod"]=localStorage.getItem("productFindingMethod");
    digitalData["position"]=localStorage.getItem("viewType")+"|"+localStorage.getItem("productIndex");
    digitalData[brandId]="";
	}
});

//calls this function when user clicks on payment modes in checkout-payment page.
/* $(document).on('click', '.tabs-left li', function(e) {
		var liValue = $(this).find('a').clone().children().remove().end().text().trim().toUpperCase();
		digitalData["paymentmode"]=liValue;
		_satellite.track('cpj_checkout_payment_selection');
	
}); */

$(document).on('click', '.pmtOptions li', function(e) {
	var liValue = $(this).find('a').clone().children().remove().end().text().trim().toUpperCase();
		digitalData["paymentmode"]=liValue;
		_satellite.track('cpj_checkout_payment_selection');
	
});

$(document).on('touchstart', '.ss-payments .pmtOptions li', function() {
	var liValue = $(this).find('a').clone().children().remove().end().text().trim().toUpperCase();
		digitalData["paymentmode"]=trackPaymentMode(liValue);
		_satellite.track('cpj_checkout_payment_selection');
});

$(document).on('click', '.popular-banks-wrap #preferredBankList li label', function() {
	var bank = $(this).closest('li').find('input').val();
	digitalData["paymentmode"]="netBanking_"+bank;
	_satellite.track('cpj_checkout_payment_selection');

});

