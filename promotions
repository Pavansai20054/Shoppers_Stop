	// var newcta_name=$(this)?.find("> a")?.prevObject[0]?.firstElementChild?.innerText
	var newcta_name=$(this)[0]?.innerText
var currentDevice = ""
		var phoneOrNot = !!navigator.maxTouchPoints;
		let phoneNum = "";
		
	if (phoneOrNot) {
		if (navigator.userAgent.match(/Android/i)) {
			currentDevice = "android"
		} else {
			currentDevice = "ios"
		}
	} else {
		currentDevice = "desktop"
	}
