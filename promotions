    function sslOSViewCategory() {
    	if (typeof _osViewCategory === "function") {
            var categoryList = "";
            $(document).ready(function () {
                var sortItem = $("#sortMobileForm1").find("#ss1 option:selected").val(), sortBy, sortOrder;
                var map = {};
                var brand = [];
                var price = [];
                var color = [];
                map["categoryRelevance"] = "categoryRelevance,de";
                map["topRated"] = "topRated,de";
                map["newArrival-desc"] = "newArrival,de";
                map["discount-desc"] = "discount,de";
                map["discount-asc"] = "discount,as";
                map["bestSeller-desc"] = "bestSeller,de";
                map["bestSeller-asc"] = "bestSeller,as";
                map["price-asc"] = "price,as";
                map["price-desc"] = "price,de";
                $.each($("input[class='facet-checkbox-mobile']:checked"), function () {
                    var facetValue = $(this).attr("data-facet-value");
                    var facetCode = $(this).attr("data-facet-heading");
                    if (facetCode === 'brands') {
                        brand.push(facetValue);
                    } else if (facetCode === 'price') {
                        facetValue.split("-").forEach(item => price.push(item));
                    } else if (facetCode === 'swatchColors') {
                        color.push(facetValue);
                    }
                });
                
                if (price.length > 0) {
                    price = price.sort(function (a, b) {
                        return a - b;
                    });
                }
                if (typeof sortItem !== 'undefined') {
                    if (typeof map[sortItem] == 'undefined') {
                        sortOrder = "as";
                        sortBy = "other";
                    } else {
                        sortBy = map[sortItem].split(',')[0];
                        sortOrder = map[sortItem].split(',')[1];
                    }
                }
                _osViewCategory({
                    "name": categoryList,
                    "minPrice": price[0],
                    "maxPrice": price[price.length - 1],
                    "sortKey": sortBy,
                    "sort": sortOrder,
                    "brands": brand,
                    "colours": color
                })
            });
        }
    }
    
    if (pageType === 'CATEGORY' || pageType === 'CATEGORYBRAND' || pageType === 'PRODUCTSEARCH') {
    	deferFunCall(_osViewCategory, sslOSViewCategory);
    }
    
    function sslOSViewL1Category() {
    	if (typeof _osViewCategory === "function") {
            _osViewCategory({
                "name": "",
            })
        }
    }

    if (pageType === 'CATEGORYLANDING') {
    	deferFunCall(_osViewCategory, sslOSViewL1Category);
    }

