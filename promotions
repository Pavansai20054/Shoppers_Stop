    if (typeof DY.API === "function") {
        var productId = $(this).closest('li.clearfix').find('.pro-img').find('a:first-child').attr('href').split("colorCode=");

        DY.API("event", {
            name: "Add to Wishlist",
            properties: {
                dyType: "add-to-wishlist-v1",
                productId: productId[1],
            }
        });
    }
 });

function trackDYAddtoCart(form,data){

    if (typeof DY.API === "function") {

        var variantColorCode = $(form).find('[data-id="styleCode"]').val();
        var size = $(form).find('[data-id="sizeValue"]').val();
        var price = $(form).find('[data-id="price"]').val();
        var jsonObj = [];
        $.each(data.entries, function () {
            jsonObj.push({
                'productId': this.product.styleCode,
                'quantity': parseInt(this.quantity),
                'itemPrice': this.product.price.value,
            });
        });
        if (typeof variantColorCode !== 'undefined' && variantColorCode !== "") {
            DY.API("event", {
                name: "Add to Cart",
                properties: {
                    dyType: "add-to-cart-v1",
                    value: parseFloat(price),
                    currency: "INR",
                    productId: variantColorCode,
                    quantity: 1,
                    size: size,
                    cart: jsonObj
                }
            });
        }
    }

}

if (localStorage.getItem("loginEvent") == "true") {
    if (typeof DY.API === "function") {
        var email = (typeof ACC.global === "undefined" || typeof ACC.global === undefined) ? "anonymous" : ACC.global.getUserData().userId;
        DY.API("event", {
            name: "Login",
            properties: {
                dyType: "login-v1",
                hashedEmail: DYO.dyhash.sha256(email),
            }
        });
        trackDYFCCLogin();
        trackDYIdentifyAPI(email);
        localStorage.removeItem("loginEvent");
    }
}

function trackDYPersonalShopperBooking() {
    if (typeof DY.API === "function") {
        DY.API("event", {
            name: "Personal Shopper Booking",
            properties: {
                type: "custom"
            }
        });
    }
}

function trackDYFirstCitizenPurchase() {
    if (typeof DY.API === "function") {
        DY.API("event", {
            name: "First Citizen Purchase",
            properties: {
                type: "custom"
            }
        });
    }
}

function trackDYFCCLogin() {
    if (typeof DY.API === "function") {
        var isFCCLinked = (typeof ACC.global === "undefined" || typeof ACC.global === undefined) ? "false" : ACC.global.getUserData().isFCClinked;
        if (typeof isFCCLinked !== undefined && isFCCLinked == 'true') {
            DY.API("event", {
                name: "First Citizen Login",
                properties: {
                    type: "custom"
                }
            });
        }
    }
}

function trackDYFCCLink() {
    if (typeof DY.API === "function") {
        DY.API("event", {
            name: "First Citizen Card Link",
            properties: {
                type: "custom"
            }
        });
    }
}

function trackDYIdentifyAPI(email) {
    if (typeof DY.API === "function") {
        DY.API("identify", {
            uid: DYO.dyhash.sha256(email), //SHA 256 hashed email of the plain text email in lower case
            type: "he"
        });
    }
}
