    var pageType = document.getElementById("pageType").value;
    var retryOsCount=0;
	function deferFunCall(osFun,hyFun) {
		retryOsCount++;
	    if(typeof osFun === "function") {
	    	hyFun();
	    } else if(retryOsCount<=5) {
	    	window.setTimeout(deferFunCall(osFun,hyFun), 100); /* this checks the flag every 100 milliseconds*/
	    }else{
	    	console.log(osFun + " not found!!");
	    }
	}

	function sslOSViewProduct() {
		if (typeof _osViewProduct === "function") {
            var category = "";
            category = category.replace("[", "").replace(/, /gi, ' > ').replace("]", "").replace("All Products > ", "");
            var discount;
            
            _osViewProduct({
                "products": [{
                    "skuId": "",
                    "sellerId": "",
                    "category": category,
                    "productPrice": parseFloat(),
                    "currency": "",
                    "discount": discount
                }]
            })

        }
	}
	
    if (pageType === 'PRODUCT') {
    	deferFunCall(_osViewProduct, sslOSViewProduct);
    }

    function extractedOsCheckout() {
        if (typeof _osCheckout === "function") {
            var products = [
                
            ];
            _osCheckout({
                products,
                "totalAmount": "",
                "currency": ""
            })
        }
    }

    // Handling quickbuy case with pagetype and document last url should be pdp.
    if (pageType === 'CHECKOUT' && document.referrer.indexOf("/p-") !== -1) {
    	deferFunCall(_osCheckout, extractedOsCheckout);
    }

    $(document).on('click', '.btn-place-order', function (e) {
        extractedOsCheckout();
    });
    
